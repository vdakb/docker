<?xml version="1.0" encoding="US-ASCII"?>
<project name="Oracle Access Manager Server Properties" basedir=".">
  <!--
  ==============================================================================
  == These properties specifies where the remote runtime environment of
  == Oracle Access Manager is located
  ==============================================================================
  -->
  <property name="oam.server.type"           value="weblogic"/>
  <property name="oam.server.protocol"       value="__OAM_PROT__"/>
  <property name="oam.server.host"           value="__OAM_HOST__"/>
  <property name="oam.server.port"           value="__OAM_PORT__"/>
  <property name="oam.server.name"           value="__OAM_NAME__"/>
  <property name="oam.server.production"     value="true"/>
  <property name="oam.server.username"       value="__OAM_USER__"/>
  <!--
  Password must be plain text. To avoid storing the server password in
  this script, it is stored in a wallet instead.
  -->
  <property name="oam.server.password"       value="__OAM_PASS__"/>
  <property name="oam.server.home"           value="__OAM_HOME__"/>
  <property name="oam.server.application"    value="__OAM_APPS__"/>
</project>