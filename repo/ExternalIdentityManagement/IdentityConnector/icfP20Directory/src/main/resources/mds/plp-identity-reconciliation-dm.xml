        <attribute type="string"  name="userPrincipalName"     source="userPrincipalName">
          <flag>ignore</flag>
        </attribute>
        <attribute type="string"  name="User Principal Name"   source="userPrincipalName">
          <flag>required</flag>
        </attribute>
        <attribute type="string"  name="xcatalogOU"            source="xcatalogOU"/>
