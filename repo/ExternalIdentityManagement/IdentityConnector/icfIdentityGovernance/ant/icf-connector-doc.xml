  </target>
  <target name="en" depends="init">
    <docbook.fop input ="en/ICF.IGS Connector Administration" style="oracle/2.0"/>
    <docbook.pdf output="ICF.IGS Connector Administration-en"/>
  </target>
  <target name="fr" depends="init">
    <docbook.fop input ="fr/ICF.IGS Connector Administration" style="oracle/2.0"/>
    <docbook.pdf output="ICF.IGS Connector Administration-fr"/>
  </target>
</project>