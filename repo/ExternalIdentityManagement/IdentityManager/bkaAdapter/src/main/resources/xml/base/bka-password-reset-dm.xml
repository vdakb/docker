      <LONGMESSAGE>&lt;html&gt;
  &lt;head&gt;&lt;meta http-equiv=&quot;content-type&quot; content=&quot;text/html; charset=UTF-8&quot;/&gt;&lt;meta name=&quot;viewport&quot; content=&quot;width=device-width, initial-scale=1&quot;&gt;&lt;style type=&quot;text/css&quot;&gt;body{font-family:Arial;width:640px;height:2rem}#content{color:#000;background-color:#fff;padding-left:3rem;padding-right:3rem}#header{color:#fff;background-color:#036;padding-left:3rem;font-size:1rem;font-weight:700;line-height:2rem;vertical-align:middle}#footer{color:#000;background-color:#fff;padding-left:3rem;font-size:.75rem;line-height:1rem;vertical-align:middle}hr{color:#036}h1.section,h2.section,h3.section,h4.section{color:#000;background-color:#fff;padding-bottom:.25rem;border-bottom:2px solid #036}h1.section{font-size:1.75rem;font-weight:700;line-height:2rem}h2.section{font-size:1.5rem;font-weight:700;line-height:1.5rem}h3.section{font-size:1.25rem;font-weight:700;line-height:1.25rem}h4.section{font-size:1rem;font-weight:700;font-style:italic;line-height:1rem}div.table{width:100%;border-collapse:collapse;display:table}div.th{color:#fff;background-color:#036;display:table-row}div.tr{display:table-row}div.td{padding-top:.5rem;padding-left:1rem;padding-right:1rem;padding-bottom:.5rem;display:table-cell}&lt;/style&gt;&lt;/head&gt;
