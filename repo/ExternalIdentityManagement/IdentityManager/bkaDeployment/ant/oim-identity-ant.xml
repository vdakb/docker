  ******************************************************************************
  -->
  <target name="0200-AN" description="Federated Identity Management Identities Police Saxony-Anhalt">
    <record name="${oim.recorder.name}" action="start" append="true" loglevel="verbose"/>
    <echo message="=================================================================="/>
