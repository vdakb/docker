    <record name="${oim.recorder.name}" action="start" append="true" loglevel="verbose"/>
    <echo message="=================================================================="/>
    <echo message="0101 Standard Entity Customization ${application.release}"/>
    <echo message="=================================================================="/>
    <echo message="Deploying importable objects for environment ${fmw.env} on ${oim.server.host}:${oim.server.port}"/>
    <object:import contextRef="oim-context">
      <object:importSet dir="0101 entityCustomization/fix">
        <object:importFile name="bka-usrentity-dm.xml"/>
      </object:importSet>
    </object:import>
    <echo message="Deploying customization sandboxes on ${mds.server.host}:${mds.server.port}/${mds.server.service}"/>
    <sandbox:import contextRef="sdb-mds">
      <sandbox:importfile publish="true" archive="0101 entityCustomization/mds/usr-entity-sysadmin.zip"/>
    </sandbox:import>
    <echo message="=================================================================="/>
    <echo message="0101 completed"/>
    <echo message="=================================================================="/>
    <record name="${oim.recorder.name}" action="stop"/>
  </target>
