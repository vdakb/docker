<?xml version="1.0" encoding="US-ASCII"?>
<project name="Oracle Identity Manager Server Properties" basedir=".">
  <!--
  ==============================================================================
  == These properties specifies where the remote runtime environment of
  == Oracle Identity Manager is located
  ==============================================================================
  -->
  <property name="oim.server.type"           value="weblogic"/>
  <property name="oim.server.protocol"       value="__OIM_PROT__"/>
  <property name="oim.server.host"           value="__OIM_HOST__"/>
  <property name="oim.server.port"           value="__OIM_PORT__"/>
  <property name="oim.server.name"           value="__OIM_NAME__"/>
  <property name="oim.server.production"     value="true"/>
  <property name="oim.server.username"       value="__OIM_USER__"/>
  <!--
  Password must be plain text. To avoid storing the server password in
  this script, it is stored in a wallet instead.
  -->
  <property name="oim.server.password"       value="__OIM_PASS__"/>
  <property name="oim.server.home"           value="__OIM_HOME__"/>
  <property name="oim.server.authentication" value="__OIM_AUTH__"/>
  <!--
  ==============================================================================
  == These properties specifies where the internal load balancer environment for
  == Identity Manager is located
  ==============================================================================
  -->
  <property name="oim.internal.host"         value="__OIM_HOST__"/>
  <property name="oim.internal.port"         value="__OIM_PORT__"/>
  <property name="oim.internal.ssl"          value="false"/>
  <!--
  ==============================================================================
  == These properties specifies where the external load balancer environment for
  == Identity Manager is located
  ==============================================================================
  -->
  <property name="oim.external.host"         value="__LBR_HOST__"/>
  <property name="oim.external.port"         value="__LBR_PORT__"/>
  <property name="oim.external.ssl"          value="false"/>
  <!--
  ==============================================================================
  == These properties specifies where the environment of metadata jmx beans of
  == Oracle Identity Manager are located
  ==============================================================================
  -->
  <property name="oim.mbean.metadata"        value="oim"/>
  <!--
  ==============================================================================
  == These properties specifies where the environment of metadata store of
  == Oracle Identity Manager is located
  ==============================================================================
  -->
  <property name="oim.metadata.partition"    value="oim"/>
  <property name="oim.sandbox.partition"     value="oim-ui"/>
</project>