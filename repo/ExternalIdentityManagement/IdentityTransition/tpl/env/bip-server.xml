<?xml version="1.0" encoding="US-ASCII"?>
<project name="BI Publisher Service Properties" basedir=".">
  <!--
  =====================================================================
  == These properties specifies where the remote runtime environment of
  == BI Publisher Application is located
  =====================================================================
  -->
  <property name="bip.service.protocol" value="__BIP_PROT__"/>
  <property name="bip.service.host"     value="__BIP_HOST__"/>
  <property name="bip.service.port"     value="__BIP_PORT__"/>
  <property name="bip.service.username" value="__BIP_USER__"/>
  <!--
  Password must be plain text. To avoid storing the server password in
  this script, it is stored in a wallet instead.
  -->
  <property name="bip.service.password" value="__BIP_PASS__"/>
  <property name="bip.service.home"     value="__BIP_HOME__"/>
  <property name="bip.service.config"   value="__BIP_CONF__"/>
</project>