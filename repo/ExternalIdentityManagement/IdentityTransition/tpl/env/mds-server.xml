<?xml version="1.0" encoding="US-ASCII"?>
<project name="Metadata Service Server Properties" basedir=".">
  <!--
  ==============================================================================
  == These properties specifies the environment of a metadata store associated
  == with a Fusion Middleware Component
  ==============================================================================
  -->
  <property name="mds.server.type"     value="oracle"/>
  <property name="mds.server.protocol" value="thin"/>
  <property name="mds.server.host"     value="__MDS_HOST__"/>
  <property name="mds.server.port"     value="__MDS_PORT__"/>
  <property name="mds.server.service"  value="__MDS_NAME__"/>
  <property name="mds.server.username" value="__MDS_USER__"/>
  <!--
  Password must be plain text. To avoid storing the server password in
  this script, it is stored in a wallet instead.
  -->
  <property name="mds.server.password" value="__MDS_PASS__"/>
</project>
