<?xml version="1.0" encoding="US-ASCII"?>
<xsd:schema xmlns               ="http://www.w3.org/2001/XMLSchema"
            xmlns:xsd           ="http://www.w3.org/2001/XMLSchema"
            xmlns:igs           ="http://www.oracle.com/schema/igs"
            targetNamespace     ="http://www.oracle.com/schema/igs"
            elementFormDefault  ="qualified"
            attributeFormDefault="unqualified"
            id                  ="organization"
            version             ="1.0.0.0">
  <xsd:include schemaLocation="entity.xsd"/>
  <!--
   | Organization
   -->
  <xsd:complexType name="organization">
    <annotation>
      <documentation>
        An organization represents a logical container of entities such as
        identities and other organizations in Identity Manager.
        Organizations in Identity Manager is used only for security purposes.

        OrganizationEntity allow you to:
          o Logically and securely manage user accounts and administrators.
          o Limit access to users, applications, roles, and entitlements.
      </documentation>
    </annotation>
    <xsd:complexContent>
      <xsd:extension base="igs:entity">
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
</xsd:schema>