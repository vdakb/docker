<?xml version="1.0" encoding="US-ASCII"?>
<project name        ="Oracle Consulting Services DocBook Definitions"
         basedir     ="../">
  <!--
  ==============================================================================
  == properties specific to the runtime environment
  ==============================================================================
  -->
  <import file="../../doc-targets.xml"/>
  <!--
  =====================================================================
  == ANT to generate product deliverables
  =====================================================================
  -->
  <target name="de" depends="init">
    <docbook.fop input ="IGS.UID Generator-de" style="p2020/2.0"/>
    <docbook.pdf output="IGS.UID Generator-de"/>
  </target>
  <target name="en" depends="init">
    <docbook.fop input ="IGS.UID Generator-en" style="p2020/2.0"/>
    <docbook.pdf output="IGS.UID Generator-en"/>
  </target>
  <target name="fr" depends="init">
    <docbook.fop input ="IGS.UID Generator-fr" style="oracle/2.0"/>
    <docbook.pdf output="IGS.UID Generator-fr"/>
  </target>
</project>