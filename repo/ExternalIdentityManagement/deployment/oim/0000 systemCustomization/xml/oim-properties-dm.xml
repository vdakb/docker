  <!--
   | Determines if notification is sent to the user when the user login and password are
   | generated in postprocess event handler for user creation via trusted source reconciliation.
   |
   | If the value is set to [TRUE|true], then notification is sent when user login and password
   | are generated in postprocess event handler for user creation via trusted source reconciliation.
   | If the value is set to [FALSE|false], then notification is not sent when user login and password
   | are generated in postprocess event handler for user creation via trusted source reconciliation.
   -->
  <SystemProperties name="Recon.SEND_NOTIFICATION" repo-type="RDBMS">
    <PTY_UPDATE>1549647340092</PTY_UPDATE>
    <PTY_VALUE>FALSE</PTY_VALUE>
    <PTY_NAME>Should send notifications in recon or not</PTY_NAME>
    <PTY_SYSTEM>1</PTY_SYSTEM>
    <PTY_DATA_LEVEL>2</PTY_DATA_LEVEL>
    <PTY_UPDATEBY>1</PTY_UPDATEBY>
    <PTY_RUN_ON>S</PTY_RUN_ON>
    <PTY_CREATEBY>1</PTY_CREATEBY>
  </SystemProperties>
  <!--
   | Specifies whether a user in the disabled state can be set as a manager for another user.
   -->
  <SystemProperties name="AllowDisabledManagers" repo-type="RDBMS">
    <PTY_UPDATE>1549647340092</PTY_UPDATE>
