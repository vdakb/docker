  <!--
   | This property enforces the validation of the user login for
   | special characters.
   -->
  <SystemProperties repo-type="RDBMS" name="XL.LoginPattern">
    <PTY_UPDATE>1707472942000</PTY_UPDATE>
    <PTY_VALUE>^[^&quot;&amp;&apos;/:&lt;&gt;@\s]{2,64}$</PTY_VALUE>
    <PTY_NAME>XL.LoginPattern</PTY_NAME>
    <PTY_SYSTEM>1</PTY_SYSTEM>
    <PTY_DATA_LEVEL>2</PTY_DATA_LEVEL>
    <PTY_UPDATEBY>1</PTY_UPDATEBY>
    <PTY_RUN_ON>S</PTY_RUN_ON>
    <PTY_CREATEBY>1</PTY_CREATEBY>
  </SystemProperties>
  <!--
   | This property contains the regular expression used to validate
   | the login of a user when XL.ValidateWhiteSpace is set to true.
   | If XL.LoginPattern is empty, then user login is validated against
   | the default pattern.
   |
   | Default value: (^[A-z0-9@._-]{2,256}$)
   -->
  <SystemProperties repo-type="RDBMS" name="XL.ValidateWhiteSpace">
    <PTY_UPDATE>1707472942000</PTY_UPDATE>
    <PTY_VALUE>TRUE</PTY_VALUE>
    <PTY_NAME>XL.ValidateWhiteSpace</PTY_NAME>
    <PTY_SYSTEM>1</PTY_SYSTEM>
    <PTY_DATA_LEVEL>2</PTY_DATA_LEVEL>
    <PTY_UPDATEBY>1</PTY_UPDATEBY>
    <PTY_RUN_ON>S</PTY_RUN_ON>
    <PTY_CREATEBY>1</PTY_CREATEBY>
  </SystemProperties>
</xl-ddm-data>
