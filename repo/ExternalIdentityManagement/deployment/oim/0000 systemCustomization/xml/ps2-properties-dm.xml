<?xml version="1.0" encoding="US-ASCII"?>
<xl-ddm-data database="" description="Federated Identity Management System Setup Configuration 12.2.1.3" exported-date="1544923160250" user="XELSYSADM" version="12.2.1.3.0">
  <!--
   | Determines if access policy engine can link access policies to reconciled accounts and to
   | accounts created by the Bulk Load Utility.
   |
   | This property is used in the context of evaluating access policies for reconciled accounts
   | and to accounts created by the Bulk Load Utility.
   -->
  <SystemProperties name="XL.AllowAPHarvesting" repo-type="RDBMS">
    <PTY_UPDATE>1544923160250</PTY_UPDATE>
    <PTY_VALUE>true</PTY_VALUE>
    <PTY_NAME>Allows linking of access policies to reconciled and bulk loaded accounts</PTY_NAME>
    <PTY_SYSTEM>1</PTY_SYSTEM>
    <PTY_DATA_LEVEL>2</PTY_DATA_LEVEL>
    <PTY_UPDATEBY>1</PTY_UPDATEBY>
    <PTY_RUN_ON>S</PTY_RUN_ON>
    <PTY_CREATEBY>1</PTY_CREATEBY>
  </SystemProperties>
  <!--
   | Determines if multiple instances of a resource can be provisioned to multiple target resources.
   |
   | When the value is [FALSE|false], provisioning multiple instances of resource object via access
   | policy is not allowed. When the value is [TRUE|true], provisioning multiple instances of
   | resource object via access policy is allowed.
   -->
  <SystemProperties name="XL.AllowAPBasedMultipleAccountProvisioning" repo-type="RDBMS">
    <PTY_UPDATE>1544923160250</PTY_UPDATE>
    <PTY_VALUE>true</PTY_VALUE>
    <PTY_NAME>Allows access policy based provisioning of multiple instances of a resource</PTY_NAME>
    <PTY_SYSTEM>1</PTY_SYSTEM>
    <PTY_DATA_LEVEL>2</PTY_DATA_LEVEL>
    <PTY_UPDATEBY>1</PTY_UPDATEBY>
    <PTY_RUN_ON>S</PTY_RUN_ON>
    <PTY_CREATEBY>1</PTY_CREATEBY>
  </SystemProperties>
  <!--
   | This property controls the amount of audit data collected when an operation is performed on a
   | role, such as creation or modification. The supported levels are:
   |
   | o None:           No audit data is collected.
   | o Role:           Creation, modification, and deletion of role is audited.
   | o Role Hierarchy: Changes made to the role inheritance is audited.
   -->
  <SystemProperties name="XL.RoleAuditLevel" repo-type="RDBMS">
    <PTY_UPDATE>1544923160250</PTY_UPDATE>
    <PTY_VALUE>Role</PTY_VALUE>
    <PTY_NAME>Level of Role Auditing</PTY_NAME>
    <PTY_SYSTEM>1</PTY_SYSTEM>
    <PTY_DATA_LEVEL>2</PTY_DATA_LEVEL>
    <PTY_UPDATEBY>1</PTY_UPDATEBY>
    <PTY_RUN_ON>C</PTY_RUN_ON>
    <PTY_CREATEBY>1</PTY_CREATEBY>
  </SystemProperties>
  <!--
   | Determines if catalog auditing is enabled or disabled. The default value is none, which specifies
   | that catalog auditing in disabled. To enable catalog auditing, set the value of this property to
   | catalog.
   -->
  <SystemProperties name="XL.CatalogAuditDataCollection" repo-type="RDBMS">
    <PTY_UPDATE>1544923160250</PTY_UPDATE>
    <PTY_VALUE>catalog</PTY_VALUE>
    <PTY_NAME>Catalog Audit Data Collection</PTY_NAME>
    <PTY_SYSTEM>1</PTY_SYSTEM>
    <PTY_DATA_LEVEL>2</PTY_DATA_LEVEL>
    <PTY_UPDATEBY>1</PTY_UPDATEBY>
    <PTY_RUN_ON>S</PTY_RUN_ON>
    <PTY_CREATEBY>1</PTY_CREATEBY>
  </SystemProperties>
</xl-ddm-data>
