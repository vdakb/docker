  <!--
   | This property is used to enable or disable blank text search in the Users, Roles,
   | Organizations, and Administration Roles page.
   |
   | If the value is [TRUE|true], then blank text search is enabled. If the value is
   | [FALSE|false], then blank text search is disabled.
   -->
  <SystemProperties name="OIG.DisableDefaultTableSearches" repo-type="RDBMS">
    <PTY_UPDATE>1614528999607</PTY_UPDATE>
    <PTY_VALUE>TRUE</PTY_VALUE>
    <PTY_NAME>Disabling Default Search of UI pages</PTY_NAME>
    <PTY_SYSTEM>1</PTY_SYSTEM>
    <PTY_DATA_LEVEL>2</PTY_DATA_LEVEL>
    <PTY_UPDATEBY>1</PTY_UPDATEBY>
    <PTY_RUN_ON>S</PTY_RUN_ON>
    <PTY_CREATEBY>1</PTY_CREATEBY>
  </SystemProperties>
  <!--
   | When the value of this property is TRUE, role lifecycle management,
   | Segregation of Duties (SoD), and identity certification are enabled.
   |
   | After modifying the value of this system property, you must restart
   | Oracle Identity Manager server for the changes to take effect.
   -->
  <SystemProperties name="OIG.IsIdentityAuditorEnabled" repo-type="RDBMS">
    <PTY_UPDATE>1544922710100</PTY_UPDATE>
    <PTY_VALUE>TRUE</PTY_VALUE>
    <PTY_NAME>Identity Auditor Feature Set Availability</PTY_NAME>
    <PTY_SYSTEM>1</PTY_SYSTEM>
    <PTY_DATA_LEVEL>2</PTY_DATA_LEVEL>
    <PTY_UPDATEBY>1</PTY_UPDATEBY>
    <PTY_RUN_ON>S</PTY_RUN_ON>
    <PTY_CREATEBY>1</PTY_CREATEBY>
  </SystemProperties>
  <!--
   | This property is used to control allowing role hierarchical access policy
   | evaluation.
   |
   | When this system property is set to [TRUE|true], access from inherited
   | access policies is given to the user. If set to [FALSE|false], access from
   | access policies attached to inherited roles is not given to the user.
   -->
  <SystemProperties name="XL.AllowRoleHierarchicalPolicyEval" repo-type="RDBMS">
    <PTY_UPDATE>1544922710100</PTY_UPDATE>
    <PTY_VALUE>TRUE</PTY_VALUE>
    <PTY_NAME>Control allowing role hierarchical access policy evaluation</PTY_NAME>
    <PTY_SYSTEM>1</PTY_SYSTEM>
    <PTY_DATA_LEVEL>2</PTY_DATA_LEVEL>
    <PTY_UPDATEBY>1</PTY_UPDATEBY>
    <PTY_RUN_ON>S</PTY_RUN_ON>
    <PTY_CREATEBY>1</PTY_CREATEBY>
  </SystemProperties>
  <!--
