<?xml version="1.0" encoding="US-ASCII"?>
<project name="Federation Proxy Server Properties" basedir=".">
  <!--
  ==============================================================================
  == These properties specifies where the remote runtime environment of
  == Oracle Identity Manager local conext is located
  ==============================================================================
  -->
  <property name="fim.server.type"       value="unifiedDirectory"/>
  <property name="fim.server.protocol"   value="__FIM_PROT__"/>
  <property name="fim.server.host"       value="__FIM_HOST__"/>
  <property name="fim.server.port"       value="__FIM_PORT__"/>
  <property name="fim.server.context"    value="__FIM_ROOT__"/>
  <property name="fim.server.username"   value="__FIM_USER__"/>
  <property name="fim.server.password"   value="__FIM_PASS__"/>
  <property name="fim.server.instance"   value="__FIM_PATH__"/>
  <!--
  =====================================================================
  == These properties specifies where the remote administration
  == environment of a Direcory Server is located
  =====================================================================
  -->
  <property name="fim.admin.port"        value="__ADM_PORT__"/>
  <property name="fim.admin.username"    value="__ADM_USER__"/>
  <property name="fim.admin.password"    value="__ADM_PASS__"/>
  <property name="fim.admin.credential"  value="__ADM_AUTH__"/>
  <!--
  =====================================================================
  == These properties specifies where the remote administration
  == environment of a Direcory Server is located
  =====================================================================
  -->
  <property name="rpl.server.port"       value="__RPL_PORT__"/>
  <!--
  ==============================================================================
  == These properties specifies the database server environment where the
  == metadata repository of Oracle Identity Manager is located.
  ==============================================================================
  -->
  <property name="dbs.server.type"      value="oracle11"/>
  <property name="dbs.server.driver"    value="oracle.jdbc.driver.OracleDriver"/>
  <property name="dbs.server.service"   value="__DBS_NAME__"/>
  <property name="dbs.server.username"  value="__DBS_USER__"/>
  <property name="dbs.server.password"  value="__DBS_PASS__"/>
  <!--
  ==============================================================================
  == These properties specifies the credentials of administrative
  == accounts.
  ==============================================================================
  -->
  <property name="ods.server.password"  value="__OUD_PASS__"/>
  <property name="iam.server.password"  value="__IAM_PASS__"/>
  <property name="oim.server.password"  value="__OIM_PASS__"/>
  <property name="oam.server.password"  value="__OAM_PASS__"/>
</project>