<antlib xmlns:object="http://oracle.iam.oauth/domain">
  <!--
  ==============================================================================
  == Register Custom Types within Ant
  ==============================================================================
  -->
  <typedef name="attribute"    classname="oracle.iam.oauth.domain.type.Attribute"/>
  <typedef name="parameter"    classname="oracle.iam.oauth.domain.type.Parameter"/>
  <typedef name="token"        classname="oracle.iam.oauth.domain.type.Token"/>
  <typedef name="tokenType"    classname="oracle.iam.oauth.domain.type.TokenType"/>
  <typedef name="tokenRefresh" classname="oracle.iam.oauth.domain.type.TokenRefresh"/>
  <typedef name="client"       classname="oracle.iam.oauth.domain.type.Client"/>
  <typedef name="resource"     classname="oracle.iam.oauth.domain.type.Resource"/>
  <typedef name="instance"     classname="oracle.iam.oauth.domain.type.Instance"/>
  <!--
  ==============================================================================
  == Register Custom Tasks within Ant
  ==============================================================================
  -->
  <taskdef name="create"       classname="oracle.iam.oauth.domain.task.Create"/>
  <taskdef name="modify"       classname="oracle.iam.oauth.domain.task.Modify"/>
  <taskdef name="delete"       classname="oracle.iam.oauth.domain.task.Delete"/>
  <taskdef name="report"       classname="oracle.iam.oauth.domain.task.Report"/>
</antlib>
