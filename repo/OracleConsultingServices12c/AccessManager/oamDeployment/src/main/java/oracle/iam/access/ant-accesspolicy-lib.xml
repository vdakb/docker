<antlib xmlns:object="http://oracle.iam.access/accesspolicy">
  <!--
  ==============================================================================
  == Register Custom Types within Ant
  ==============================================================================
  -->
  <typedef name="uri"                  classname="oracle.iam.access.common.type.URI"/>
  <typedef name="addedUri"             classname="oracle.iam.access.common.type.URI$Added"/>
  <typedef name="deletedUri"           classname="oracle.iam.access.common.type.URI$Deleted"/>
  <typedef name="url"                  classname="oracle.iam.access.common.type.Resource"/>
  <typedef name="resourcePublic"       classname="oracle.iam.access.common.type.Resource$Public"/>
  <typedef name="resourceProtected"    classname="oracle.iam.access.common.type.Resource$Protected"/>
  <typedef name="variations"           classname="oracle.iam.access.common.type.Variation"/>
  <typedef name="variation"            classname="oracle.iam.access.common.type.Variation$Server"/>
  <typedef name="conditions"           classname="oracle.iam.access.common.type.Conditions"/>
  <typedef name="condition"            classname="oracle.iam.access.common.type.Conditions$Condition"/>
  <typedef name="ip"                   classname="oracle.iam.access.common.type.Conditions$Condition$IP"/>
  <typedef name="range"                classname="oracle.iam.access.common.type.Conditions$Condition$IP$Range"/>
  <typedef name="temporal"             classname="oracle.iam.access.common.type.Conditions$Condition$Temporal"/>
  <typedef name="dayOfWeek"            classname="oracle.iam.access.common.type.Conditions$Condition$Temporal$DayOfWeek"/>
  <typedef name="identity"             classname="oracle.iam.access.common.type.Conditions$Condition$Identity"/>
  <typedef name="attribute"            classname="oracle.iam.access.common.type.Conditions$Condition$Attribute"/>
  <typedef name="rules"                classname="oracle.iam.access.common.type.Rules"/>
  <typedef name="rule"                 classname="oracle.iam.access.common.type.Rules$Rule"/>
  <typedef name="combiner"             classname="oracle.iam.access.common.type.Rules$Rule$Combiner"/>
  <typedef name="mode"                 classname="oracle.iam.access.common.type.Rules$Rule$Combiner$Mode"/>
  <typedef name="conditionNames"       classname="oracle.iam.access.common.type.Rules$Rule$Combiner$Mode$ConditionNames"/>
  <typedef name="applicationDomain"    classname="oracle.iam.access.common.type.ApplicationDomain"/>
  <typedef name="authentication"       classname="oracle.iam.access.common.type.ApplicationDomain$Authentication"/>
  <typedef name="authenticationPolicy" classname="oracle.iam.access.common.type.ApplicationDomain$Authentication$Policy"/>
  <typedef name="authorization"        classname="oracle.iam.access.common.type.ApplicationDomain$Authorization"/>
  <typedef name="authorizationPolicy"  classname="oracle.iam.access.common.type.ApplicationDomain$Authorization$Policy"/>
  <typedef name="response"             classname="oracle.iam.access.common.type.ApplicationDomain$Response"/>
  <typedef name="responseSuccess"      classname="oracle.iam.access.common.type.ApplicationDomain$Response$Success"/>

  <typedef name="parameter"            classname="oracle.iam.access.policy.type.Parameter"/>
  <typedef name="instance"             classname="oracle.iam.access.policy.type.Instance"/>
  <!--
  ==============================================================================
  == Register Custom Tasks within Ant
  ==============================================================================
  -->
  <taskdef name="create"               classname="oracle.iam.access.policy.task.Create"/>
  <taskdef name="modify"               classname="oracle.iam.access.policy.task.Modify"/>
  <taskdef name="delete"               classname="oracle.iam.access.policy.task.Delete"/>
</antlib>
