<antlib xmlns:object="http://oracle.iam.access/accessservice">
  <!--
  ==============================================================================
  == Register Custom Types within Ant
  ==============================================================================
  -->
  <typedef name="service" classname="oracle.iam.access.accessservice.type.Service"/>
  <!--
  ==============================================================================
  == Register Custom Tasks within Ant
  ==============================================================================
  -->
  <taskdef name="enable"  classname="oracle.iam.access.accessservice.task.Enable"/>
  <taskdef name="disable" classname="oracle.iam.access.accessservice.task.Disable"/>
  <taskdef name="status"  classname="oracle.iam.access.accessservice.task.Status"/>
</antlib>
