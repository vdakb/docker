<antlib xmlns:object="http://oracle.iam.access/identitystore">
  <!--
  ==============================================================================
  == Register Custom Types within Ant
  ==============================================================================
  -->
  <typedef name="parameter" classname="oracle.iam.access.identitystore.type.Parameter"/>
  <typedef name="instance"  classname="oracle.iam.access.identitystore.type.Instance"/>
  <!--
  ==============================================================================
  == Register Custom Tasks within Ant
  ==============================================================================
  -->
  <taskdef name="create"    classname="oracle.iam.access.identitystore.task.Create"/>
  <taskdef name="modify"    classname="oracle.iam.access.identitystore.task.Modify"/>
  <taskdef name="delete"    classname="oracle.iam.access.identitystore.task.Delete"/>
  <taskdef name="print"     classname="oracle.iam.access.identitystore.task.Report"/>
</antlib>
