<antlib xmlns:object="http://oracle.iam.oauth/resource">
  <!--
  ==============================================================================
  == Register Custom Types within Ant
  ==============================================================================
  -->
  <typedef name="type"           classname="oracle.iam.oauth.resource.type.TokenAttributeType"/>
  <typedef name="scope"          classname="oracle.iam.oauth.resource.type.Scope"/>
  <typedef name="domain"         classname="oracle.iam.oauth.resource.type.Domain"/>
  <typedef name="parameter"      classname="oracle.iam.oauth.resource.type.Parameter"/>
  <typedef name="instance"       classname="oracle.iam.oauth.resource.type.Instance"/>
  <typedef name="audienceClaim"  classname="oracle.iam.oauth.resource.type.AudienceClaim"/>
  <typedef name="tokenAttribute" classname="oracle.iam.oauth.resource.type.TokenAttribute"/>
  <!--
  ==============================================================================
  == Register Custom Tasks within Ant
  ==============================================================================
  -->
  <taskdef name="create"         classname="oracle.iam.oauth.resource.task.Create"/>
  <taskdef name="modify"         classname="oracle.iam.oauth.resource.task.Modify"/>
  <taskdef name="delete"         classname="oracle.iam.oauth.resource.task.Delete"/>
  <taskdef name="report"         classname="oracle.iam.oauth.resource.task.Report"/>
</antlib>
