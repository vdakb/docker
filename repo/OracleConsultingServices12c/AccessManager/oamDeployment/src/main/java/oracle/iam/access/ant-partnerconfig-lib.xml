<antlib xmlns:object="http://oracle.iam.access/partnerconfig">
  <!--
  ==============================================================================
  == Register Custom Types within Ant
  ==============================================================================
  -->
  <typedef name="type"      classname="oracle.iam.access.partnerconfig.type.PartnerType"/>
  <typedef name="partner"   classname="oracle.iam.access.partnerconfig.type.Partner"/>
  <typedef name="parameter" classname="oracle.iam.access.partnerconfig.type.Parameter"/>
  <!--
  ==============================================================================
  == Register Custom Tasks within Ant
  ==============================================================================
  -->
  <taskdef name="status"    classname="oracle.iam.access.partnerconfig.task.Status"/>
  <taskdef name="create"    classname="oracle.iam.access.partnerconfig.task.Create"/>
  <taskdef name="modify"    classname="oracle.iam.access.partnerconfig.task.Modify"/>
  <taskdef name="delete"    classname="oracle.iam.access.partnerconfig.task.Delete"/>
  <taskdef name="print"     classname="oracle.iam.access.partnerconfig.task.Report"/>
</antlib>
