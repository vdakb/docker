<antlib xmlns:object="http://oracle.iam.access/accessagent">
  <!--
  ==============================================================================
  == Register Custom Types within Ant
  ==============================================================================
  -->
  <typedef name="server"                classname="oracle.iam.access.common.type.AccessServer"/>
  <typedef name="serverPrimary"         classname="oracle.iam.access.common.type.AccessServer$Primary"/>
  <typedef name="serverSecondary"       classname="oracle.iam.access.common.type.AccessServer$Secondary"/>
  <typedef name="uri"                   classname="oracle.iam.access.common.type.URI"/>
  <typedef name="addedUri"              classname="oracle.iam.access.common.type.URI$Added"/>
  <typedef name="deletedUri"            classname="oracle.iam.access.common.type.URI$Deleted"/>
  <typedef name="url"                   classname="oracle.iam.access.common.type.Resource"/>
  <typedef name="loginURL"              classname="oracle.iam.access.common.type.Resource$Login"/>
  <typedef name="logoutURL"             classname="oracle.iam.access.common.type.Resource$Logout"/>
  <typedef name="resourcePublic"        classname="oracle.iam.access.common.type.Resource$Public"/>
  <typedef name="resourceExcluded"      classname="oracle.iam.access.common.type.Resource$Excluded"/>
  <typedef name="resourceProtected"     classname="oracle.iam.access.common.type.Resource$Protected"/>
  <typedef name="resourceNotEnforced"   classname="oracle.iam.access.common.type.Resource$NotEnforced"/>
  <typedef name="variations"            classname="oracle.iam.access.common.type.Variation"/>
  <typedef name="variation"             classname="oracle.iam.access.common.type.Variation$Server"/>

  <typedef name="version"               classname="oracle.iam.access.accessagent.type.Version"/>
  <typedef name="parameter"             classname="oracle.iam.access.accessagent.type.Parameter"/>
  <typedef name="instance"              classname="oracle.iam.access.accessagent.type.Instance"/>
  <typedef name="address"               classname="oracle.iam.access.accessagent.type.ValidationException$Address"/>
  <typedef name="userDefined"           classname="oracle.iam.access.accessagent.type.UserProperty"/>
  <typedef name="userValue"             classname="oracle.iam.access.accessagent.type.UserProperty$Property$Value"/>
  <typedef name="userProperty"          classname="oracle.iam.access.accessagent.type.UserProperty$Property"/>
  <typedef name="validationException"   classname="oracle.iam.access.accessagent.type.ValidationException"/>
  <typedef name="mapping"               classname="oracle.iam.access.accessagent.type.Mapping"/>
  <typedef name="profileMapping"        classname="oracle.iam.access.accessagent.type.Mapping$Profile"/>
  <typedef name="sessionMapping"        classname="oracle.iam.access.accessagent.type.Mapping$Session"/>
  <typedef name="responseMapping"       classname="oracle.iam.access.accessagent.type.Mapping$Response"/>
  <typedef name="miscellaneous"         classname="oracle.iam.access.accessagent.type.Miscellaneous"/>
  <typedef name="miscellaneousProperty" classname="oracle.iam.access.accessagent.type.Miscellaneous"/>
  <!--
  ==============================================================================
  == Register Custom Tasks within Ant
  ==============================================================================
  -->
  <taskdef name="create"                classname="oracle.iam.access.accessagent.task.Create"/>
  <taskdef name="modify"                classname="oracle.iam.access.accessagent.task.Modify"/>
  <taskdef name="delete"                classname="oracle.iam.access.accessagent.task.Delete"/>
  <taskdef name="print"                 classname="oracle.iam.access.accessagent.task.Report"/>
  <taskdef name="validate"              classname="oracle.iam.access.accessagent.task.Validate"/>
</antlib>
