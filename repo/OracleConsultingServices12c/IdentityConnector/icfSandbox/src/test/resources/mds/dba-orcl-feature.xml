<?xml version="1.0" encoding="US-ASCII"?>
<!--
===========================================================
Extended Feature Configuration for the database flavor of
the Openfire Database Connector
===========================================================
-->
<feature xmlns         ="http://www.oracle.com/schema/iam/endpoint"
         bundle-entry  ="oracle.iam.identity.icf.connector.dbs.Main"
         bundle-name   ="dbs.identity.connector.bundle"
         bundle-version="12.2.1.3">
  <property name="database-system-timestamp" value="select systimestamp from dual"/>
  <property name="pseudo-rownum-attribute"   value="rownum"/>
  <property name="entry-created-attribute"   value="creationdate"/>
  <property name="entry-modified-attribute"  value="modificationdate"/>
</feature>