<?xml version="1.0" encoding="US-ASCII"?>
<web-app xmlns             ="http://java.sun.com/xml/ns/javaee" version="2.5"
         xmlns:xsi         ="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <mime-mapping>
    <extension>png</extension>
    <mime-type>image/png</mime-type>
  </mime-mapping>
  <mime-mapping>
    <extension>gif</extension>
    <mime-type>image/gif</mime-type>
  </mime-mapping>
  <mime-mapping>
    <extension>swf</extension>
    <mime-type>application/x-shockwave-flash</mime-type>
  </mime-mapping>
  <mime-mapping>
    <extension>js</extension>
    <mime-type>application/javascript</mime-type>
  </mime-mapping>
  <mime-mapping>
    <extension>ttf</extension>
    <mime-type>application/x-font-ttf</mime-type>
  </mime-mapping>
  <mime-mapping>
    <extension>eot</extension>
    <mime-type>application/vnd.ms-fontobject</mime-type>
  </mime-mapping>
  <mime-mapping>
    <extension>otf</extension>
    <mime-type>application/x-font-opentype</mime-type>
  </mime-mapping>
  <mime-mapping>
    <extension>woff</extension>
    <mime-type>application/x-font-woff</mime-type>
  </mime-mapping>
  <mime-mapping>
    <extension>woff2</extension>
    <mime-type>application/x-font-woff2</mime-type>
  </mime-mapping>
  <servlet>
    <servlet-name>signin</servlet-name>
    <jsp-file>/WEB-INF/pages/signin.jsp</jsp-file>
  </servlet>
  <servlet>
    <servlet-name>signup</servlet-name>
    <jsp-file>/WEB-INF/pages/signup.jsp</jsp-file>
  </servlet>
  <servlet>
    <servlet-name>signout</servlet-name>
    <jsp-file>/WEB-INF/pages/signout.jsp</jsp-file>
  </servlet>
  <servlet>
    <servlet-name>forgot-username</servlet-name>
    <jsp-file>/WEB-INF/pages/forgot-username.jsp</jsp-file>
  </servlet>
  <servlet>
    <servlet-name>forgot-password</servlet-name>
    <jsp-file>/WEB-INF/pages/forgot-password.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>signin</servlet-name>
    <url-pattern>/signin</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>signup</servlet-name>
    <url-pattern>/signup</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>signout</servlet-name>
    <url-pattern>/signout</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>forgot-username</servlet-name>
    <url-pattern>/forgot-username</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>forgot-password</servlet-name>
    <url-pattern>/forgot-password</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>/signin</welcome-file>
  </welcome-file-list>
  <session-config>
    <session-timeout>10</session-timeout>
  </session-config>
</web-app>
