<antlib xmlns:object="http://oracle.iam.identity/job">
  <!--
  ==============================================================================
  == Register Custom Types within Ant
  ==============================================================================
  -->
  <typedef name="job"               classname="oracle.iam.identity.scheduler.type.Job"/>
  <typedef name="trigger"           classname="oracle.iam.identity.scheduler.type.Schedule"/>
  <typedef name="none"              classname="oracle.iam.identity.scheduler.type.Schedule$None"/>
  <typedef name="single"            classname="oracle.iam.identity.scheduler.type.Schedule$Single"/>
  <typedef name="simple"            classname="oracle.iam.identity.scheduler.type.Schedule$Simple"/>
  <typedef name="cron"              classname="oracle.iam.identity.scheduler.type.Schedule$Cron"/>
  <typedef name="parameter"         classname="oracle.iam.identity.scheduler.type.Parameter"/>
  <typedef name="parameterproperty" classname="oracle.iam.identity.scheduler.type.Parameter$Property"/>
  <typedef name="start"             classname="oracle.iam.identity.scheduler.type.Start"/>
  <typedef name="stop"              classname="oracle.iam.identity.scheduler.type.Stop"/>
  <typedef name="status"            classname="oracle.iam.identity.scheduler.type.Status"/>
  <!--
  ==============================================================================
  == Register Custom Tasks within Ant
  ==============================================================================
  -->
</antlib>
