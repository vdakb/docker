<antlib xmlns:object="http://oracle.iam.identity/operation">
  <!--
  ==============================================================================
  == Register Custom Types within Ant
  ==============================================================================
  -->
  <typedef name="element"        classname="oracle.iam.identity.deployment.type.Element"/>
  <typedef name="parameter"      classname="oracle.iam.identity.deployment.type.Parameter"/>
  <typedef name="dataSet"        classname="oracle.iam.identity.deployment.type.RequestDataSet"/>
  <typedef name="attribute"      classname="oracle.iam.identity.deployment.type.RequestDataSet$Attribute"/>
  <typedef name="resource"       classname="oracle.iam.identity.deployment.type.AdminRole$Resource"/>
  <typedef name="capability"     classname="oracle.iam.identity.deployment.type.AdminRole$Capability"/>
  <typedef name="membership"     classname="oracle.iam.identity.deployment.type.MemberShip"/>
  <typedef name="recipient"      classname="oracle.iam.identity.deployment.type.MemberShip$Recipient"/>
  <!--
  <typedef name="policy"         classname="oracle.iam.identity.deployment.type.AccessPolicy"/>
  <typedef name="provision"      classname="oracle.iam.identity.deployment.type.AccessPolicy$Allow"/>
  <typedef name="trigger"        classname="oracle.iam.identity.deployment.type.AccessPolicy$Trigger"/>
  <typedef name="deny"           classname="oracle.iam.identity.deployment.type.AccessPolicy$Deny"/>
  <typedef name="form"           classname="oracle.iam.identity.deployment.type.ProcessForm"/>
  -->
  <!--
  ==============================================================================
  == Register Custom Tasks within Ant
  ==============================================================================
  -->
  <taskdef name="systemproperty" classname="oracle.iam.identity.deployment.task.SystemPropertyOperation"/>
  <taskdef name="lookup"         classname="oracle.iam.identity.deployment.task.LookupOperation"/>
  <taskdef name="itresource"     classname="oracle.iam.identity.deployment.task.ITResourceOperation"/>
  <taskdef name="role"           classname="oracle.iam.identity.deployment.task.RoleOperation"/>
  <taskdef name="adminrole"      classname="oracle.iam.identity.deployment.task.AdminRoleOperation"/>
  <taskdef name="organization"   classname="oracle.iam.identity.deployment.task.OrganizationOperation"/>
  <taskdef name="user"           classname="oracle.iam.identity.deployment.task.UserProfileOperation"/>
  <taskdef name="accesspolicy"   classname="oracle.iam.identity.deployment.task.AccessPolicyOperation"/>
  <taskdef name="scheduler"      classname="oracle.iam.identity.deployment.task.SchedulerOperation"/>
  <taskdef name="catalog"        classname="oracle.iam.identity.deployment.task.CatalogOperation"/>
  <taskdef name="application"    classname="oracle.iam.identity.deployment.task.ApplicationOperation"/>
  <taskdef name="entitlement"    classname="oracle.iam.identity.deployment.task.EntitlementOperation"/>
  <taskdef name="policy"         classname="oracle.iam.identity.deployment.task.AccessPolicyOperation"/>
</antlib>