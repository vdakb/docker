<antlib xmlns:object="http://oracle.iam.identity/metadata">
  <!--
  ==============================================================================
  == Register Custom Types within Ant
  ==============================================================================
  -->
  <typedef name="path"          classname="oracle.iam.identity.metadata.type.Path"/>
  <typedef name="pathelement"   classname="oracle.iam.identity.metadata.type.Path$PathElement"/>
  <typedef name="folderSet"     classname="oracle.iam.identity.metadata.type.FolderSet"/>
  <typedef name="exportSet"     classname="oracle.iam.identity.metadata.type.ExportSet"/>
  <typedef name="importSet"     classname="oracle.iam.identity.metadata.type.ImportSet"/>
  <typedef name="importFile"    classname="oracle.iam.identity.metadata.type.ImportFile"/>
  <!--
  ==============================================================================
  == Register Custom Tasks within Ant
  ==============================================================================
  -->
  <taskdef name="clearcache"    classname="oracle.iam.identity.metadata.task.ClearCache"/>
  <taskdef name="purge"         classname="oracle.iam.identity.metadata.task.Purge"/>
  <taskdef name="export"        classname="oracle.iam.identity.metadata.task.Export"/>
  <taskdef name="exportVersion" classname="oracle.iam.identity.metadata.task.ExportVersion"/>
  <taskdef name="import"        classname="oracle.iam.identity.metadata.task.Import"/>
  <taskdef name="delete"        classname="oracle.iam.identity.metadata.task.Delete"/>
  <taskdef name="upload"        classname="oracle.iam.identity.metadata.task.Upload"/>
  <taskdef name="download"      classname="oracle.iam.identity.metadata.task.Download"/>
  <taskdef name="remove"        classname="oracle.iam.identity.metadata.task.Remove"/>
</antlib>