<antlib xmlns:object="http://oracle.iam.identity/sandbox">
  <!--
  ==============================================================================
  == Register Custom Types within Ant
  ==============================================================================
  -->
  <typedef name="form"       classname="oracle.iam.identity.sandbox.type.Form"/>
  <typedef name="bundle"     classname="oracle.iam.identity.sandbox.type.Bundle"/>
  <typedef name="account"    classname="oracle.iam.identity.sandbox.type.Account"/>
  <typedef name="sandbox"    classname="oracle.iam.identity.sandbox.type.Sandbox"/>
  <typedef name="attribute"  classname="oracle.iam.identity.sandbox.type.Attribute"/>
  <typedef name="instance"   classname="oracle.iam.identity.sandbox.type.Instance"/>
  <typedef name="exportFile" classname="oracle.iam.identity.sandbox.type.ExportFile"/>
  <typedef name="importFile" classname="oracle.iam.identity.sandbox.type.ImportFile"/>
  <typedef name="usage"      classname="oracle.iam.identity.sandbox.type.Usage"/>
  <typedef name="catalogAM"  classname="oracle.iam.identity.sandbox.type.CatalogAM"/>
  <!--
  ==============================================================================
  == Register Custom Tasks within Ant
  ==============================================================================
  -->
  <taskdef name="export"          classname="oracle.iam.identity.sandbox.task.Export"/>
  <taskdef name="import"          classname="oracle.iam.identity.sandbox.task.Import"/>
  <taskdef name="delete"          classname="oracle.iam.identity.sandbox.task.Delete"/>
  <taskdef name="publish"         classname="oracle.iam.identity.sandbox.task.Publish"/>
  <taskdef name="commit"          classname="oracle.iam.identity.sandbox.task.Commit"/>
  <taskdef name="rollback"        classname="oracle.iam.identity.sandbox.task.Rollback"/>
  <taskdef name="generate"        classname="oracle.iam.identity.sandbox.task.Generate"/>
  <taskdef name="generateCatalog" classname="oracle.iam.identity.sandbox.task.GenerateCatalog"/>
</antlib>
