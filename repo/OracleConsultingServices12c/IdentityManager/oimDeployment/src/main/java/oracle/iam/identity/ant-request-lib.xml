<antlib xmlns:object="http://oracle.iam.identity/request">
  <!--
  ==============================================================================
  == Register Custom Types within Ant
  ==============================================================================
  -->
  <typedef name="resource"         classname="oracle.iam.identity.request.type.Resource"/>
  <typedef name="process"          classname="oracle.iam.identity.request.type.Process"/>
  <typedef name="operation"        classname="oracle.iam.identity.request.type.Operation"/>
  <typedef name="requestattribute" classname="oracle.iam.identity.request.type.Attribute"/>
  <typedef name="entityattribute"  classname="oracle.iam.identity.request.type.AttributeReference"/>
  <typedef name="lookupvalue"      classname="oracle.iam.identity.request.type.LookupValues"/>
  <typedef name="lookupquery"      classname="oracle.iam.identity.request.type.LookupQuery"/>
  <typedef name="validator"        classname="oracle.iam.identity.request.type.DataSetValidator"/>
  <typedef name="widget"           classname="oracle.iam.identity.request.type.Widget"/>
  <typedef name="variant"          classname="oracle.iam.identity.request.type.Variant"/>
  <!--
  ==============================================================================
  == Register Custom Tasks within Ant
  ==============================================================================
  -->
  <taskdef name="generateDataSet"  classname="oracle.iam.identity.workflow.task.DataSetGenerator"/>
</antlib>