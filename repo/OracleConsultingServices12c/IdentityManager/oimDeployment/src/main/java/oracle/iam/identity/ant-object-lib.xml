<antlib xmlns:object="http://oracle.iam.identity/object">
  <!--
  ==============================================================================
  == Register Custom Types within Ant
  ==============================================================================
  -->
  <typedef name="category"     classname="oracle.iam.identity.deployment.type.Category"/>
  <typedef name="exportSet"    classname="oracle.iam.identity.deployment.type.ExportSet"/>
  <typedef name="importSet"    classname="oracle.iam.identity.deployment.type.ImportSet"/>
  <typedef name="importFile"   classname="oracle.iam.identity.deployment.type.ImportFile"/>
  <typedef name="substitution" classname="oracle.iam.identity.deployment.type.Substitution"/>
  <!--
  ==============================================================================
  == Register Custom Tasks within Ant
  ==============================================================================
  -->
  <taskdef name="purgecache"   classname="oracle.iam.identity.deployment.task.PurgeCache"/>
  <taskdef name="export"       classname="oracle.iam.identity.deployment.task.Export"/>
  <taskdef name="import"       classname="oracle.iam.identity.deployment.task.Import"/>
</antlib>
