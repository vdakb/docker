<?xml version="1.0" encoding="US-ASCII"?>
<descriptor xmlns         ="http://www.oracle.com/schema/oim/mapping"
            identifier    ="User ID"
            transformation="false"
            natively      ="false">

  <attribute   name="User ID"           source="uid"/>
  <attribute   name="Title"             source="title"/>
  <attribute   name="First Name"        source="givenname"/>
  <attribute   name="Last Name"         source="sn"/>
  <attribute   name="Middle Name"       source="middlename"/>
  <attribute   name="Initials"          source="initials"/>
  <attribute   name="Organization"      source="o"/>
  <attribute   name="Language"          source="preferredlanguage"/>
  <attribute   name="Mail Address"      source="mail"/>
  <attribute   name="Telephone Number"  source="telephonenumber"/>
  <attribute   name="Facsimile Number"  source="facsimiletelephonenumber"/>
  <attribute   name="Mobile Number"     source="mobile"/>
  <attribute   name="Pager Number"      source="pager"/>
  <attribute   name="Country"           source="c"/>
  <attribute   name="State"             source="st"/>
  <attribute   name="Locality"          source="l"/>
  <attribute   name="Street"            source="street"/>
  <attribute   name="Postal Address"    source="postalcode"/>
  <attribute   name="Postal Code"       source="postaladdress"/>
</descriptor>
