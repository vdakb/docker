<?xml version="1.0" encoding="US-ASCII"?>
<schema xmlns             ="http://www.w3.org/2001/XMLSchema"
        xmlns:evidence    ="http://xmlns.oracle.com/bpel/workflow/TaskEvidenceService"
        xmlns:jaxb        ="http://java.sun.com/xml/ns/jaxb" jaxb:version="1.0"
        xmlns:tns         ="http://xmlns.oracle.com/bpel/workflow/task"
        targetNamespace   ="http://xmlns.oracle.com/bpel/workflow/task"
        blockDefault      ="#all"
        elementFormDefault="qualified">

  <include xmlns="" schemaLocation="approval-task-payload.xsd"/>

  <annotation>
    <appinfo>
      <jaxb:globalBindings generateElementProperty="false"/>
      <jaxb:schemaBindings>
        <jaxb:package name="oracle.bpel.services.workflow.task.model"/>
      </jaxb:schemaBindings>
    </appinfo>
  </annotation>

  <import namespace="http://xmlns.oracle.com/bpel/workflow/TaskEvidenceService" schemaLocation="oramds:///soa/shared/workflow/TaskEvidenceService.xsd"/>

  <element name="task">
    <complexType>
      <sequence>
        <element name="title"                   type="string" minOccurs="0"/>
        <element name="payload"                 type="tns:approvalPayloadType" minOccurs="0" maxOccurs="1"/>
        <element name="taskDefinitionURI"       type="string" minOccurs="0"/>
        <!-- Deprecated: Use taskDefinitionId instead -->
        <element name="creator"                 type="string"                          minOccurs="0"/>
        <element name="ownerUser"               type="string"                          minOccurs="0"/>
        <element name="ownerGroup"              type="string"                          minOccurs="0"/>
        <element name="ownerRole"               type="string"                          minOccurs="0"/>
        <element name="priority"                type="tns:priorityType"                minOccurs="0"/>
        <element name="packageName"             type="string"                          minOccurs="0" maxOccurs="1"/>
        <element name="identityContext"         type="string"                          minOccurs="0"/>
        <element name="userComment"             type="tns:commentType"                 minOccurs="0" maxOccurs="unbounded"/>
        <element name="attachment"              type="tns:attachmentType"              minOccurs="0" maxOccurs="unbounded"/>
        <element name="document"                type="tns:documentType"                minOccurs="0" maxOccurs="unbounded"/>
        <element name="processInfo"             type="tns:processType"                 minOccurs="0"/>
        <element name="systemAttributes"        type="tns:systemAttributesType"        minOccurs="0"/>
        <element name="systemMessageAttributes" type="tns:systemMessageAttributesType" minOccurs="0"/>
        <element name="titleResourceKey"        type="string"                          minOccurs="0"/>
        <element name="callback"                type="tns:callbackType"                minOccurs="0"/>
        <element name="identificationKey"       type="string"                          minOccurs="0"/>
        <element name="category"                type="string"                          minOccurs="0"/>
        <element name="startDate"               type="dateTime"                        minOccurs="0"/>
        <element name="dueDate"                 type="dateTime"                        minOccurs="0"/>
        <element name="isPublic"                type="boolean"                         minOccurs="0"/>
        <element name="percentageComplete"      type="double"                          minOccurs="0"/>
        <element name="sca"                     type="tns:scaType"                     minOccurs="0"/>
        <element name="applicationContext"      type="string"                          minOccurs="0"/>
        <element name="evidence"                type="evidence:EvidenceType"           minOccurs="0" maxOccurs="1"/>
        <element name="taskDefinitionId"        type="string"                          minOccurs="0"/>
        <element name="subcategory"             type="string"                          minOccurs="0"/>
        <element name="taskDisplayUrl"          type="string"                          minOccurs="0"/>
        <element name="documentPackageId"       type="string"                          minOccurs="0"/>
        <element name="correlationId"           type="string"                          minOccurs="0"/>
        <element name="mdsLabel"                type="string"                          minOccurs="0"/>
        <element name="creatorDisplayName"      type="string"                          minOccurs="0"/>
        <element name="ownerUserDisplayName"    type="string"                          minOccurs="0"/>
        <element name="ownerGroupDisplayName"   type="string"                          minOccurs="0"/>
        <element name="ownerRoleDisplayName"    type="string"                          minOccurs="0"/>
        <element name="customAttributes"        type="tns:customAttributesType"        minOccurs="0"/>
        <element name="shortSummary"            type="string"                          minOccurs="0"/>
        <element name="longSummary"             type="string"                          minOccurs="0"/>
        <element name="notificationMode"        type="tns:taskNotificationMode"        minOccurs="0"/>
      </sequence>
    </complexType>
  </element>
  <element name="comment" type="tns:commentType"/>
  <complexType name="commentType">
    <sequence>
      <element name="comment"             type="string"                minOccurs="0"/>
      <element name="updatedBy"           type="tns:identityType"      minOccurs="0"/>
      <element name="updatedDate"         type="dateTime"              minOccurs="0"/>
      <element name="displayNameLanguage" type="string"                minOccurs="0"/>
      <element name="action"              type="string"                minOccurs="0"/>
      <element name="systemVersionFlag"   type="string"                minOccurs="0"/>
      <element name="acl"                 type="string"                minOccurs="0"/>
      <element name="doesBelongToParent"  type="boolean"               minOccurs="0"/>
      <element name="isSystemComment"     type="boolean"               minOccurs="0"/>
      <element name="taskId"              type="string"                minOccurs="0"/>
      <element name="commentScope"        type="tns:scopeTypeEnum"     minOccurs="0"/>
      <element name="id"                  type="string"                minOccurs="0" maxOccurs="1"/>
      <element name="operation"           type="tns:operationTypeEnum" minOccurs="0" maxOccurs="1"/>
    </sequence>
  </complexType>
  <complexType name="processType">
    <sequence>
      <element name="domainId"       type="string"              minOccurs="0" maxOccurs="1"/>
      <element name="instanceId"     type="string"              minOccurs="0" maxOccurs="1"/>
      <element name="processId"      type="string"              minOccurs="0" maxOccurs="1"/>
      <element name="processName"    type="string"              minOccurs="0" maxOccurs="1"/>
      <element name="processType"    type="tns:processTypeEnum" minOccurs="0" maxOccurs="1"/>
      <element name="processVersion" type="string"              minOccurs="0" maxOccurs="1"/>
    </sequence>
  </complexType>
  <complexType name="scaType">
    <sequence>
      <element name="applicationName"              type="string" minOccurs="0" maxOccurs="1"/>
      <element name="componentName"                type="string" minOccurs="0" maxOccurs="1"/>
      <element name="compositeDN"                  type="string" minOccurs="0" maxOccurs="1"/>
      <element name="compositeInstanceId"          type="string" minOccurs="0" maxOccurs="1"/>
      <element name="compositeName"                type="string" minOccurs="0" maxOccurs="1"/>
      <element name="compositeVersion"             type="string" minOccurs="0" maxOccurs="1"/>
      <element name="ecId"                         type="string" minOccurs="0" maxOccurs="1"/>
      <element name="parentComponentInstanceId"    type="string" minOccurs="0" maxOccurs="1"/>
      <element name="parentComponentInstanceRefId" type="string" minOccurs="0" maxOccurs="1"/>
      <element name="headerProperties"             type="string" minOccurs="0" maxOccurs="1"/>
      <element name="invokedComponent"             type="string" minOccurs="0" maxOccurs="1">
        <annotation>
          <documentation>
              Records the component invoked to create this Task version instance.
              E.g. the decision component invoked for advanced routing rules.
            </documentation>
        </annotation>
      </element>
      <element name="compositeCreatedTime" type="dateTime" minOccurs="0" maxOccurs="1"/>
      <element name="componentInstanceId"  type="long"     minOccurs="0" maxOccurs="1"/>
      <element name="flowId"               type="long"     minOccurs="0" maxOccurs="1"/>
      <element name="scaPartitionId"       type="long"     minOccurs="0" maxOccurs="1"/>
    </sequence>
  </complexType>
  <simpleType name="processTypeEnum">
    <restriction base="string">
      <enumeration value="BPEL"/>
      <enumeration value="OWF"/>
      <enumeration value="OTHER"/>
    </restriction>
  </simpleType>
  <complexType name="callbackType">
    <sequence>
      <element name="context"         type="string" minOccurs="0"/>
      <element name="id"              type="string" minOccurs="0"/>
      <element name="type"            type="string" minOccurs="0"/>
      <element name="converstationId" type="string" minOccurs="0" maxOccurs="1"/>
    </sequence>
  </complexType>
  <element name="attachment" type="tns:attachmentType"/>
  <complexType name="attachmentType">
    <sequence>
      <element name="content"              type="string"                  minOccurs="0"/>
      <element name="mimeType"             type="string"                  minOccurs="0"/>
      <element name="name"                 type="string"                  minOccurs="0"/>
      <element name="updatedBy"            type="string"                  minOccurs="0"/>
      <element name="URI"                  type="string"                  minOccurs="0"/>
      <element name="systemVersionFlag"    type="string"                  minOccurs="0"/>
      <element name="taskId"               type="string"                  minOccurs="0"/>
      <element name="version"              type="integer"                 minOccurs="0"/>
      <element name="acl"                  type="string"                  minOccurs="0"/>
      <element name="doesBelongToParent"   type="boolean"                 minOccurs="0"/>
      <element name="updatedDate"          type="dateTime"                minOccurs="0"/>
      <element name="correlationId"        type="string"                  minOccurs="0" maxOccurs="1"/>
      <element name="size"                 type="long"                    minOccurs="0" maxOccurs="1"/>
      <element name="description"          type="string"                  minOccurs="0" maxOccurs="1"/>
      <element name="storageType"          type="tns:storageTypeEnum"     minOccurs="0" maxOccurs="1"/>
      <element name="ucmDocType"           type="string"                  minOccurs="0" maxOccurs="1"/>
      <element name="securityGroup"        type="string"                  minOccurs="0" maxOccurs="1"/>
      <element name="account"              type="string"                  minOccurs="0" maxOccurs="1"/>
      <element name="revision"             type="string"                  minOccurs="0" maxOccurs="1"/>
      <element name="releaseDate"          type="date"                    minOccurs="0" maxOccurs="1"/>
      <element name="expirationDate"       type="date"                    minOccurs="0" maxOccurs="1"/>
      <element name="title"                type="string"                  minOccurs="0" maxOccurs="1"/>
      <element name="ucmMetadataItem"      type="tns:ucmMetadataItemType" minOccurs="0" maxOccurs="unbounded"/>
      <element name="attachmentScope"      type="tns:scopeTypeEnum"       minOccurs="0"/>
      <element name="isContentEncoded"     type="boolean"                 minOccurs="0" maxOccurs="1"/>
      <element name="updatedByDisplayName" type="string"                  minOccurs="0"/>
      <element name="id"                   type="string"                  minOccurs="0" maxOccurs="1"/>
      <element name="operation"            type="tns:operationTypeEnum"   minOccurs="0" maxOccurs="1"/>
    </sequence>
  </complexType>
  <complexType name="ucmMetadataItemType">
    <sequence>
      <element name="name"  type="string"               minOccurs="0" maxOccurs="1"/>
      <element name="value" type="string"               minOccurs="0" maxOccurs="1"/>
      <element name="type"  type="tns:metadataTypeEnum" minOccurs="0" maxOccurs="1"/>
    </sequence>
  </complexType>
  <simpleType name="scopeTypeEnum">
    <restriction base="string">
      <enumeration value="TASK"/>
      <enumeration value="PARENT_TASK"/>
      <enumeration value="BPM"/>
      <enumeration value="CASE"/>
    </restriction>
  </simpleType>
  <simpleType name="operationTypeEnum">
    <restriction base="string">
      <enumeration value="ADD"/>
      <enumeration value="UPDATE"/>
      <enumeration value="DELETE"/>
    </restriction>
  </simpleType>
  <simpleType name="storageTypeEnum">
    <restriction base="string">
      <enumeration value="TASK"/>
      <enumeration value="UCM"/>
      <enumeration value="URL"/>
    </restriction>
  </simpleType>
  <simpleType name="metadataTypeEnum">
    <restriction base="string">
      <enumeration value="DATE"/>
      <enumeration value="STRING"/>
      <enumeration value="NUMBER"/>
    </restriction>
  </simpleType>
  <element name="document" type="tns:documentType"/>
  <complexType name="documentType">
    <sequence>
      <element name="name"            type="string"/>
      <element name="content"         type="string"   minOccurs="0"/>
      <element name="mimeType"        type="string"   minOccurs="0"/>
      <element name="URI"             type="string"/>
      <element name="updatedBy"       type="string"   minOccurs="0"/>
      <element name="updatedDate"     type="dateTime" minOccurs="0"/>
      <element name="ucmDocumentType" type="string"   minOccurs="0"/>
      <element name="UUID"            type="string"/>
    </sequence>
  </complexType>
  <complexType name="identityType">
    <sequence>
      <element name="id"                type="string"               minOccurs="0"/>
      <element name="displayName"       type="string"               minOccurs="0"/>
      <element name="type"              type="tns:identityTypeEnum" minOccurs="0"/>
      <element name="systemVersionFlag" type="string"               minOccurs="0"/>
    </sequence>
  </complexType>
  <simpleType name="identityTypeEnum">
    <restriction base="string">
      <enumeration value="user"/>
      <enumeration value="group"/>
      <enumeration value="application_role"/>
      <enumeration value="position"/>
    </restriction>
  </simpleType>
  <complexType name="hiddenAttributesType">
    <sequence>
      <element name="isCreatorHidden"   type="boolean" minOccurs="0" maxOccurs="1"/>
      <element name="creatorCustomText" type="string"  minOccurs="0" maxOccurs="1"/>
      <element name="isAutoApproval"    type="boolean" minOccurs="0" maxOccurs="1"/>
    </sequence>
  </complexType>
  <complexType name="systemAttributesType">
    <sequence>
      <element name="accessKey"                       type="string"                     minOccurs="0"/>
      <element name="acquiredBy"                      type="string"                     minOccurs="0"/>
      <element name="approvalDuration"                type="long"                       minOccurs="0"/>
      <element name="approvers"                       type="string"                     minOccurs="0"/>
      <element name="assignedDate"                    type="dateTime"                   minOccurs="0"/>
      <element name="assigneeGroups"                  type="tns:identityType"           minOccurs="0" maxOccurs="unbounded"/>
      <element name="assigneeUsers"                   type="tns:identityType"           minOccurs="0" maxOccurs="unbounded"/>
      <element name="createdDate"                     type="dateTime"                   minOccurs="0"/>
      <element name="customActions"                   type="tns:actionType"             minOccurs="0" maxOccurs="unbounded"/>
      <element name="digitalSignatureRequired"        type="boolean"                    minOccurs="0"/>
      <element name="displayInfo"                     type="tns:displayInfoType"        minOccurs="0" maxOccurs="unbounded"/>
      <element name="elapsedTime"                     type="long"                       minOccurs="0"/>
      <element name="endDate"                         type="dateTime"                   minOccurs="0"/>
      <element name="expirationDate"                  type="dateTime"                   minOccurs="0"/>
      <element name="expirationDuration"              type="duration"                   minOccurs="0"/>
      <element name="fromUser"                        type="tns:identityType"           minOccurs="0"/>
      <element name="hasSubTasks"                     type="boolean"                    minOccurs="0"/>
      <element name="inShortHistory"                  type="boolean"                    minOccurs="0"/>
      <element name="isGroup"                         type="boolean"                    minOccurs="0"/>
      <element name="language"                        type="string"                     minOccurs="0"/>
      <element name="mailStatus"                      type="tns:mailStatusEnum"         minOccurs="0"/>
      <element name="numberOfTimesModified"           type="integer"                    minOccurs="0"/>
      <element name="originalAssigneeUser"            type="string"                     minOccurs="0"/>
      <element name="outcome"                         type="string"                     minOccurs="0"/>
      <element name="parallelOutcomeCount"            type="string"                     minOccurs="0"/>
      <element name="passwordRequiredOnUpdate"        type="boolean"                    minOccurs="0"/>
      <element name="pushbackSequence"                type="string"                     minOccurs="0"/>
      <element name="secureNotifications"             type="boolean"                    minOccurs="0"/>
      <element name="shortHistory"                    type="tns:shortHistoryType"       minOccurs="0"/>
      <element name="state"                           type="tns:stateEnum"              minOccurs="0"/>
      <element name="substate"                        type="tns:substateEnum"           minOccurs="0"/>
      <element name="systemActions"                   type="tns:actionType"             minOccurs="0" maxOccurs="unbounded"/>
      <element name="systemString1"                   type="string"                     minOccurs="0"/>
      <element name="systemString2"                   type="string"                     minOccurs="0"/>
      <element name="systemString3"                   type="string"                     minOccurs="0"/>
      <element name="taskGroupId"                     type="string"                     minOccurs="0"/>
      <element name="taskId"                          type="string"                     minOccurs="0"/>
      <element name="taskNumber"                      type="integer"                    minOccurs="0"/>
      <element name="updatedBy"                       type="tns:identityType"           minOccurs="0"/>
      <element name="updatedDate"                     type="dateTime"                   minOccurs="0"/>
      <element name="version"                         type="integer"                    minOccurs="0"/>
      <element name="versionReason"                   type="string"                     minOccurs="0"/>
      <element name="taskDefinitionId"                type="string"                     minOccurs="0"/>
      <!-- This is deprecated and moved as an attribute that can be set by clients -->
      <element name="taskDefinitionName"              type="string"                     minOccurs="0"/>
      <element name="workflowPattern"                 type="string"                     minOccurs="0"/>
      <element name="isTestTask"                      type="boolean"                    minOccurs="0"/>
      <element name="participantName"                 type="string"                     minOccurs="0"/>
      <element name="readByUsers"                     type="string"                     minOccurs="0"/>
      <element name="reviewers"                       type="tns:identityType"           minOccurs="0" maxOccurs="unbounded"/>
      <element name="assignees"                       type="tns:identityType"           minOccurs="0" maxOccurs="unbounded"/>
      <element name="taskGroupInstanceId"             type="string"                     minOccurs="0"/>
      <element name="subTaskGroupInstanceId"          type="string"                     minOccurs="0"/>
      <element name="parentTaskVersion"               type="integer"                    minOccurs="0"/>
      <!-- Activity Guide elements -->
      <element name="agRootId"                        type="long"                       minOccurs="0"/>
      <element name="agMileStonePath"                 type="string"                     minOccurs="0"/>
      <element name="rootTaskId"                      type="string"                     minOccurs="0"/>
      <element name="parentTaskId"                    type="string"                     minOccurs="0"/>
      <element name="systemStringActions"             type="string"                     minOccurs="0"/>
      <element name="restrictedActions"               type="tns:actionType"             minOccurs="0" maxOccurs="unbounded"/>
      <element name="updatedNotificationId"           type="string"                     minOccurs="0"/>
      <element name="stage"                           type="string"                     minOccurs="0"/>
      <element name="assignmentContext"               type="tns:assignmentContextType"  minOccurs="0" maxOccurs="unbounded"/>
      <element ref="tns:collectionTarget"                                               minOccurs="0" maxOccurs="unbounded"/>
      <element name="preActionUserSteps"              type="tns:preActionUserStepsType" minOccurs="0" maxOccurs="1"/>
      <element name="aggregationTaskId"               type="string"                     minOccurs="0" maxOccurs="1"/>
      <element name="isTemplateTask"                  type="boolean"                    minOccurs="0" maxOccurs="1"/>
      <element name="taskViewContext"                 type="string"                     minOccurs="0" maxOccurs="1"/>
      <element name="taskNamespace"                   type="string"                     minOccurs="0"/>
      <element name="actionDisplayName"               type="string"                     minOccurs="0" maxOccurs="1"/>
      <element name="timers"                          type="string"                     minOccurs="0" maxOccurs="1"/>
      <element name="componentType"                   type="string"                     minOccurs="0" maxOccurs="1"/>
      <element name="activityName"                    type="string"                     minOccurs="0" maxOccurs="1"/>
      <element name="activityId"                      type="string"                     minOccurs="0" maxOccurs="1"/>
      <element name="processDueDate"                  type="dateTime"                   minOccurs="0" maxOccurs="1"/>
      <element name="thread"                          type="integer"                    minOccurs="0" maxOccurs="1"/>
      <element name="parentThread"                    type="integer"                    minOccurs="0" maxOccurs="1"/>
      <element name="step"                            type="integer"                    minOccurs="0" maxOccurs="1"/>
      <element name="visibilityRuleConditionValues"   type="string"                     minOccurs="0" maxOccurs="1"/>
      <element name="isPartialTask"                   type="boolean"                    minOccurs="0" maxOccurs="1"/>
      <element name="organizationalUnitId"            type="string"                     minOccurs="0" maxOccurs="1"/>
      <element name="tenantId"                        type="string"                     minOccurs="0" maxOccurs="1"/>
      <element name="listBuilderId"                   type="string"                     minOccurs="0" maxOccurs="1"/>
      <element name="caseId"                          type="string"                     minOccurs="0" maxOccurs="1"/>
      <element name="organizationalUnitName"          type="string"                     minOccurs="0" maxOccurs="1"/>
      <element name="swimlaneRole"                    type="string"                     minOccurs="0" maxOccurs="1"/>
      <element name="excludedParticipants"            type="string"                     minOccurs="0" maxOccurs="1"/>
      <element name="timersSuspended"                 type="boolean"                    minOccurs="0" maxOccurs="1" default="false"/>
      <element name="tenantIdNumber"                  type="long"                       minOccurs="0" maxOccurs="1" default="-1"/>
      <element name="taskAgeFromCreatedDate"          type="duration"                   minOccurs="0" maxOccurs="1"/>
      <element name="taskAgeFromAssignedDate"         type="duration"                   minOccurs="0" maxOccurs="1"/>
      <element name="taskTimeRemaining"               type="duration"                   minOccurs="0" maxOccurs="1"/>
      <element name="isDecomposedTask"                type="boolean"                    minOccurs="0" maxOccurs="1" default="false"/>
      <element name="formName"                        type="string"                     minOccurs="0" maxOccurs="1"/>
      <element name="decomposedTaskGroupInstanceId"   type="string"                     minOccurs="0" maxOccurs="1"/>
      <element name="startTime"                       type="dateTime"                   minOccurs="0" maxOccurs="1"/>
      <element name="stopTime"                        type="dateTime"                   minOccurs="0" maxOccurs="1"/>
      <element name="workingDuration"                 type="integer"                    minOccurs="0" maxOccurs="1"/>
      <element name="acquiredByDisplayName"           type="string"                     minOccurs="0" maxOccurs="1"/>
      <element name="originalAssigneeUserDisplayName" type="string"                     minOccurs="0" maxOccurs="1"/>
      <element name="approversDisplayNames"           type="string"                     minOccurs="0" maxOccurs="1"/>
      <element name="imageUrl"                        type="string"                     minOccurs="0" maxOccurs="1"/>
      <element name="hiddenAttributes"                type="tns:hiddenAttributesType"   minOccurs="0" maxOccurs="1"/>
    </sequence>
  </complexType>
  <element name="collectionTarget" type="tns:collectionTargetType"/>
  <complexType name="collectionTargetType">
    <sequence>
      <element name="id"             type="string"                       minOccurs="1" maxOccurs="1"/>
      <element name="xpath"          type="string"                       minOccurs="1" maxOccurs="1"/>
      <element name="collectionName" type="string"                       minOccurs="1" maxOccurs="1"/>
      <element name="type"           type="tns:collectionTargetTypeEnum" minOccurs="1" maxOccurs="1"/>
      <choice minOccurs="1" maxOccurs="1">
        <element name="targetIndex" type="integer"/>
        <element ref="tns:keyList"/>
      </choice>
      <element name="referencedTaskId"  type="string"                         minOccurs="0" maxOccurs="1"/>
      <element name="taskAggregationId" type="string"                         minOccurs="0" maxOccurs="1"/>
      <element name="action"            type="tns:collectionTargetActionType" minOccurs="0" maxOccurs="1"/>
      <element name="status"            type="string"                         minOccurs="0" maxOccurs="1"/>
      <element name="displayName"       type="string"                         minOccurs="0" maxOccurs="1"/>
    </sequence>
  </complexType>
  <simpleType name="collectionTargetTypeEnum">
    <restriction base="string">
      <enumeration value="PRIMARY"/>
      <enumeration value="AGGREGATED"/>
    </restriction>
  </simpleType>
  <element name="keyList" type="tns:keyListType"/>
  <complexType name="keyListType">
    <sequence>
      <element name="key" type="tns:keyType" minOccurs="1" maxOccurs="unbounded"/>
    </sequence>
  </complexType>
  <complexType name="keyType">
    <sequence>
      <element name="name"  type="string" minOccurs="1" maxOccurs="1"/>
      <element name="value" type="string" minOccurs="1" maxOccurs="1"/>
    </sequence>
  </complexType>
  <complexType name="collectionTargetActionType">
    <simpleContent>
      <extension base="string">
        <attribute name="name"      type="string" use="required"/>
        <attribute name="params"    type="string" use="optional"/>
        <attribute name="completed" type="boolean" use="optional"/>
      </extension>
    </simpleContent>
  </complexType>
  <complexType name="actionType">
    <sequence>
      <element name="action"      type="string" minOccurs="0"/>
      <element name="displayName" type="string" minOccurs="0"/>
    </sequence>
  </complexType>
  <complexType name="shortHistoryType">
    <sequence>
      <element name="task" type="tns:shortHistoryTaskType" minOccurs="0" maxOccurs="unbounded"/>
    </sequence>
  </complexType>
  <complexType name="shortHistoryTaskType">
    <sequence>
      <element name="state"         type="string"           minOccurs="0"/>
      <element name="outcome"       type="string"           minOccurs="0"/>
      <element name="comments"      type="string"           minOccurs="0"/>
      <element name="updatedBy"     type="tns:identityType" minOccurs="0"/>
      <element name="updatedDate"   type="dateTime"         minOccurs="0"/>
      <element name="version"       type="integer"          minOccurs="0"/>
      <element name="versionReason" type="string"           minOccurs="0"/>
    </sequence>
  </complexType>
  <complexType name="displayInfoType">
    <sequence>
