<?xml version = '1.0' encoding = 'UTF-8'?>
<xsd:schema targetNamespace="http://xmlns.oracle.com/bpel/workflow/task" xmlns:tns="http://xmlns.oracle.com/bpel/workflow/task" xmlns:evidence="http://xmlns.oracle.com/bpel/workflow/TaskEvidenceService" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:jaxb="http://java.sun.com/xml/ns/jaxb" jaxb:version="1.0" blockDefault="#all" elementFormDefault="qualified">
  <xsd:include xmlns="" schemaLocation="approval-task-payload.xsd"/>
  <xsd:annotation>
    <xsd:appinfo>
      <jaxb:globalBindings generateElementProperty="false"/>
      <jaxb:schemaBindings>
        <jaxb:package name="oracle.bpel.services.workflow.task.model"/>
      </jaxb:schemaBindings>
    </xsd:appinfo>
  </xsd:annotation>

  <xsd:import namespace="http://xmlns.oracle.com/bpel/workflow/TaskEvidenceService" schemaLocation="oramds:///soa/shared/workflow/TaskEvidenceService.xsd"/>

  <xsd:element name="task">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element name="title" type="xsd:string" minOccurs="0"/>
        <xsd:element name="payload" type="tns:approvalPayloadType" minOccurs="0" maxOccurs="1"/>
        <xsd:element name="taskDefinitionURI" type="xsd:string" minOccurs="0"/>
        <!-- Deprecated: Use taskDefinitionId instead -->
        <xsd:element name="creator" type="xsd:string" minOccurs="0"/>
        <xsd:element name="ownerUser" type="xsd:string" minOccurs="0"/>
        <xsd:element name="ownerGroup" type="xsd:string" minOccurs="0"/>
        <xsd:element name="ownerRole" type="xsd:string" minOccurs="0"/>
        <xsd:element name="priority" type="tns:priorityType" minOccurs="0"/>
        <xsd:element name="packageName" type="xsd:string" minOccurs="0" maxOccurs="1"/>
        <xsd:element name="identityContext" type="xsd:string" minOccurs="0"/>
        <xsd:element name="userComment" type="tns:commentType" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element name="attachment" type="tns:attachmentType" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element name="document" type="tns:documentType" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element name="processInfo" type="tns:processType" minOccurs="0"/>
        <xsd:element name="systemAttributes" type="tns:systemAttributesType" minOccurs="0"/>
        <xsd:element name="systemMessageAttributes" type="tns:systemMessageAttributesType" minOccurs="0"/>
        <xsd:element name="titleResourceKey" type="xsd:string" minOccurs="0"/>
        <xsd:element name="callback" type="tns:callbackType" minOccurs="0"/>
        <xsd:element name="identificationKey" type="xsd:string" minOccurs="0"/>
        <xsd:element name="category" type="xsd:string" minOccurs="0"/>
        <xsd:element name="startDate" type="xsd:dateTime" minOccurs="0"/>
        <xsd:element name="dueDate" type="xsd:dateTime" minOccurs="0"/>
        <xsd:element name="isPublic" type="xsd:boolean" minOccurs="0"/>
        <xsd:element name="percentageComplete" type="xsd:double" minOccurs="0"/>
        <xsd:element name="sca" type="tns:scaType" minOccurs="0"/>
        <xsd:element name="applicationContext" type="xsd:string" minOccurs="0"/>
        <xsd:element name="evidence" type="evidence:EvidenceType" minOccurs="0" maxOccurs="1"/>
        <xsd:element name="taskDefinitionId" type="xsd:string" minOccurs="0"/>
        <xsd:element name="subcategory" type="xsd:string" minOccurs="0"/>
        <xsd:element name="taskDisplayUrl" type="xsd:string" minOccurs="0"/>
        <xsd:element name="documentPackageId" type="xsd:string" minOccurs="0"/>
        <xsd:element name="correlationId" type="xsd:string" minOccurs="0"/>
        <xsd:element name="mdsLabel" type="xsd:string" minOccurs="0"/>
        <xsd:element name="creatorDisplayName" type="xsd:string" minOccurs="0"/>
        <xsd:element name="ownerUserDisplayName" type="xsd:string" minOccurs="0"/>
        <xsd:element name="ownerGroupDisplayName" type="xsd:string" minOccurs="0"/>
        <xsd:element name="ownerRoleDisplayName" type="xsd:string" minOccurs="0"/>
        <xsd:element name="customAttributes" type="tns:customAttributesType" minOccurs="0"/>
        <xsd:element name="shortSummary" type="xsd:string" minOccurs="0"/>
        <xsd:element name="longSummary" type="xsd:string" minOccurs="0"/>
        <xsd:element name="notificationMode" type="tns:taskNotificationMode" minOccurs="0"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="comment" type="tns:commentType"/>
  <xsd:complexType name="commentType">
    <xsd:sequence>
      <xsd:element name="comment" type="xsd:string" minOccurs="0"/>
      <xsd:element name="updatedBy" type="tns:identityType" minOccurs="0"/>
      <xsd:element name="updatedDate" type="xsd:dateTime" minOccurs="0"/>
      <xsd:element name="displayNameLanguage" type="xsd:string" minOccurs="0"/>
      <xsd:element name="action" type="xsd:string" minOccurs="0"/>
      <xsd:element name="systemVersionFlag" type="xsd:string" minOccurs="0"/>
      <xsd:element name="acl" type="xsd:string" minOccurs="0"/>
      <xsd:element name="doesBelongToParent" type="xsd:boolean" minOccurs="0"/>
      <xsd:element name="isSystemComment" type="xsd:boolean" minOccurs="0"/>
      <xsd:element name="taskId" type="xsd:string" minOccurs="0"/>
      <xsd:element name="commentScope" type="tns:scopeTypeEnum" minOccurs="0"/>
      <xsd:element name="id" type="xsd:string" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="operation" type="tns:operationTypeEnum" minOccurs="0" maxOccurs="1"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="processType">
    <xsd:sequence>
      <xsd:element name="domainId" type="xsd:string" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="instanceId" type="xsd:string" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="processId" type="xsd:string" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="processName" type="xsd:string" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="processType" type="tns:processTypeEnum" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="processVersion" type="xsd:string" minOccurs="0" maxOccurs="1"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="scaType">
    <xsd:sequence>
      <xsd:element name="applicationName" type="xsd:string" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="componentName" type="xsd:string" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="compositeDN" type="xsd:string" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="compositeInstanceId" type="xsd:string" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="compositeName" type="xsd:string" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="compositeVersion" type="xsd:string" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="ecId" type="xsd:string" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="parentComponentInstanceId" type="xsd:string" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="parentComponentInstanceRefId" type="xsd:string" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="headerProperties" type="xsd:string" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="invokedComponent" type="xsd:string" minOccurs="0" maxOccurs="1">
        <xsd:annotation>
          <xsd:documentation>
              Records the component invoked to create this Task version instance.
              E.g. the decision component invoked for advanced routing rules.
            </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="compositeCreatedTime" type="xsd:dateTime" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="componentInstanceId" type="xsd:long" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="flowId" type="xsd:long" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="scaPartitionId" type="xsd:long" minOccurs="0" maxOccurs="1"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:simpleType name="processTypeEnum">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="BPEL"/>
      <xsd:enumeration value="OWF"/>
      <xsd:enumeration value="OTHER"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="callbackType">
    <xsd:sequence>
      <xsd:element name="context" type="xsd:string" minOccurs="0"/>
      <xsd:element name="id" type="xsd:string" minOccurs="0"/>
      <xsd:element name="type" type="xsd:string" minOccurs="0"/>
      <xsd:element name="converstationId" type="xsd:string" minOccurs="0" maxOccurs="1"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:element name="attachment" type="tns:attachmentType"/>
  <xsd:complexType name="attachmentType">
    <xsd:sequence>
      <xsd:element name="content" type="xsd:string" minOccurs="0"/>
      <xsd:element name="mimeType" type="xsd:string" minOccurs="0"/>
      <xsd:element name="name" type="xsd:string" minOccurs="0"/>
      <xsd:element name="updatedBy" type="xsd:string" minOccurs="0"/>
      <xsd:element name="URI" type="xsd:string" minOccurs="0"/>
      <xsd:element name="systemVersionFlag" type="xsd:string" minOccurs="0"/>
      <xsd:element name="taskId" type="xsd:string" minOccurs="0"/>
      <xsd:element name="version" type="xsd:integer" minOccurs="0"/>
      <xsd:element name="acl" type="xsd:string" minOccurs="0"/>
      <xsd:element name="doesBelongToParent" type="xsd:boolean" minOccurs="0"/>
      <xsd:element name="updatedDate" type="xsd:dateTime" minOccurs="0"/>
      <xsd:element name="correlationId" type="xsd:string" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="size" type="xsd:long" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="description" type="xsd:string" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="storageType" type="tns:storageTypeEnum" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="ucmDocType" type="xsd:string" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="securityGroup" type="xsd:string" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="account" type="xsd:string" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="revision" type="xsd:string" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="releaseDate" type="xsd:date" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="expirationDate" type="xsd:date" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="title" type="xsd:string" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="ucmMetadataItem" type="tns:ucmMetadataItemType" minOccurs="0" maxOccurs="unbounded"/>
      <xsd:element name="attachmentScope" type="tns:scopeTypeEnum" minOccurs="0"/>
      <xsd:element name="isContentEncoded" type="xsd:boolean" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="updatedByDisplayName" type="xsd:string" minOccurs="0"/>
      <xsd:element name="id" type="xsd:string" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="operation" type="tns:operationTypeEnum" minOccurs="0" maxOccurs="1"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="ucmMetadataItemType">
    <xsd:sequence>
      <xsd:element name="name" type="xsd:string" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="value" type="xsd:string" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="type" type="tns:metadataTypeEnum" minOccurs="0" maxOccurs="1"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:simpleType name="scopeTypeEnum">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="TASK"/>
      <xsd:enumeration value="PARENT_TASK"/>
      <xsd:enumeration value="BPM"/>
      <xsd:enumeration value="CASE"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="operationTypeEnum">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="ADD"/>
      <xsd:enumeration value="UPDATE"/>
      <xsd:enumeration value="DELETE"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="storageTypeEnum">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="TASK"/>
      <xsd:enumeration value="UCM"/>
      <xsd:enumeration value="URL"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="metadataTypeEnum">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="DATE"/>
      <xsd:enumeration value="STRING"/>
      <xsd:enumeration value="NUMBER"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:element name="document" type="tns:documentType"/>
  <xsd:complexType name="documentType">
    <xsd:sequence>
      <xsd:element name="name" type="xsd:string"/>
      <xsd:element name="content" type="xsd:string" minOccurs="0"/>
      <xsd:element name="mimeType" type="xsd:string" minOccurs="0"/>
      <xsd:element name="URI" type="xsd:string"/>
      <xsd:element name="updatedBy" type="xsd:string" minOccurs="0"/>
      <xsd:element name="updatedDate" type="xsd:dateTime" minOccurs="0"/>
      <xsd:element name="ucmDocumentType" type="xsd:string" minOccurs="0"/>
      <xsd:element name="UUID" type="xsd:string"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="identityType">
    <xsd:sequence>
      <xsd:element name="id" type="xsd:string" minOccurs="0"/>
      <xsd:element name="displayName" type="xsd:string" minOccurs="0"/>
      <xsd:element name="type" type="tns:identityTypeEnum" minOccurs="0"/>
      <xsd:element name="systemVersionFlag" type="xsd:string" minOccurs="0"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:simpleType name="identityTypeEnum">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="user"/>
      <xsd:enumeration value="group"/>
      <xsd:enumeration value="application_role"/>
      <xsd:enumeration value="position"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="hiddenAttributesType">
    <xsd:sequence>
      <xsd:element name="isCreatorHidden" type="xsd:boolean" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="creatorCustomText" type="xsd:string" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="isAutoApproval" type="xsd:boolean" minOccurs="0" maxOccurs="1"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="systemAttributesType">
    <xsd:sequence>
      <xsd:element name="accessKey" type="xsd:string" minOccurs="0"/>
      <xsd:element name="acquiredBy" type="xsd:string" minOccurs="0"/>
      <xsd:element name="approvalDuration" type="xsd:long" minOccurs="0"/>
      <xsd:element name="approvers" type="xsd:string" minOccurs="0"/>
      <xsd:element name="assignedDate" type="xsd:dateTime" minOccurs="0"/>
      <xsd:element name="assigneeGroups" type="tns:identityType" minOccurs="0" maxOccurs="unbounded"/>
      <xsd:element name="assigneeUsers" type="tns:identityType" minOccurs="0" maxOccurs="unbounded"/>
      <xsd:element name="createdDate" type="xsd:dateTime" minOccurs="0"/>
      <xsd:element name="customActions" type="tns:actionType" minOccurs="0" maxOccurs="unbounded"/>
      <xsd:element name="digitalSignatureRequired" type="xsd:boolean" minOccurs="0"/>
      <xsd:element name="displayInfo" type="tns:displayInfoType" minOccurs="0" maxOccurs="unbounded"/>
      <xsd:element name="elapsedTime" type="xsd:long" minOccurs="0"/>
      <xsd:element name="endDate" type="xsd:dateTime" minOccurs="0"/>
      <xsd:element name="expirationDate" type="xsd:dateTime" minOccurs="0"/>
      <xsd:element name="expirationDuration" type="xsd:duration" minOccurs="0"/>
      <xsd:element name="fromUser" type="tns:identityType" minOccurs="0"/>
      <xsd:element name="hasSubTasks" type="xsd:boolean" minOccurs="0"/>
      <xsd:element name="inShortHistory" type="xsd:boolean" minOccurs="0"/>
      <xsd:element name="isGroup" type="xsd:boolean" minOccurs="0"/>
      <xsd:element name="language" type="xsd:string" minOccurs="0"/>
      <xsd:element name="mailStatus" type="tns:mailStatusEnum" minOccurs="0"/>
      <xsd:element name="numberOfTimesModified" type="xsd:integer" minOccurs="0"/>
      <xsd:element name="originalAssigneeUser" type="xsd:string" minOccurs="0"/>
      <xsd:element name="outcome" type="xsd:string" minOccurs="0"/>
      <xsd:element name="parallelOutcomeCount" type="xsd:string" minOccurs="0"/>
      <xsd:element name="passwordRequiredOnUpdate" type="xsd:boolean" minOccurs="0"/>
      <xsd:element name="pushbackSequence" type="xsd:string" minOccurs="0"/>
      <xsd:element name="secureNotifications" type="xsd:boolean" minOccurs="0"/>
      <xsd:element name="shortHistory" type="tns:shortHistoryType" minOccurs="0"/>
      <xsd:element name="state" type="tns:stateEnum" minOccurs="0"/>
      <xsd:element name="substate" type="tns:substateEnum" minOccurs="0"/>
      <xsd:element name="systemActions" type="tns:actionType" minOccurs="0" maxOccurs="unbounded"/>
      <xsd:element name="systemString1" type="xsd:string" minOccurs="0"/>
      <xsd:element name="systemString2" type="xsd:string" minOccurs="0"/>
      <xsd:element name="systemString3" type="xsd:string" minOccurs="0"/>
      <xsd:element name="taskGroupId" type="xsd:string" minOccurs="0"/>
      <xsd:element name="taskId" type="xsd:string" minOccurs="0"/>
      <xsd:element name="taskNumber" type="xsd:integer" minOccurs="0"/>
      <xsd:element name="updatedBy" type="tns:identityType" minOccurs="0"/>
      <xsd:element name="updatedDate" type="xsd:dateTime" minOccurs="0"/>
      <xsd:element name="version" type="xsd:integer" minOccurs="0"/>
      <xsd:element name="versionReason" type="xsd:string" minOccurs="0"/>
      <xsd:element name="taskDefinitionId" type="xsd:string" minOccurs="0"/>
      <!-- This is deprecated and moved as an attribute that can be set by clients -->
      <xsd:element name="taskDefinitionName" type="xsd:string" minOccurs="0"/>
      <xsd:element name="workflowPattern" type="xsd:string" minOccurs="0"/>
      <xsd:element name="isTestTask" type="xsd:boolean" minOccurs="0"/>
      <xsd:element name="participantName" type="xsd:string" minOccurs="0"/>
      <xsd:element name="readByUsers" type="xsd:string" minOccurs="0"/>
      <xsd:element name="reviewers" type="tns:identityType" minOccurs="0" maxOccurs="unbounded"/>
      <xsd:element name="assignees" type="tns:identityType" minOccurs="0" maxOccurs="unbounded"/>
      <xsd:element name="taskGroupInstanceId" type="xsd:string" minOccurs="0"/>
      <xsd:element name="subTaskGroupInstanceId" type="xsd:string" minOccurs="0"/>
      <xsd:element name="parentTaskVersion" type="xsd:integer" minOccurs="0"/>
      <!-- Activity Guide elements -->
      <xsd:element name="agRootId" type="xsd:long" minOccurs="0"/>
      <xsd:element name="agMileStonePath" type="xsd:string" minOccurs="0"/>
      <xsd:element name="rootTaskId" type="xsd:string" minOccurs="0"/>
      <xsd:element name="parentTaskId" type="xsd:string" minOccurs="0"/>
      <xsd:element name="systemStringActions" type="xsd:string" minOccurs="0"/>
      <xsd:element name="restrictedActions" type="tns:actionType" minOccurs="0" maxOccurs="unbounded"/>
      <xsd:element name="updatedNotificationId" type="xsd:string" minOccurs="0"/>
      <xsd:element name="stage" type="xsd:string" minOccurs="0"/>
      <xsd:element name="assignmentContext" type="tns:assignmentContextType" minOccurs="0" maxOccurs="unbounded"/>
      <xsd:element ref="tns:collectionTarget" minOccurs="0" maxOccurs="unbounded"/>
      <xsd:element name="preActionUserSteps" type="tns:preActionUserStepsType" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="aggregationTaskId" type="xsd:string" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="isTemplateTask" type="xsd:boolean" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="taskViewContext" type="xsd:string" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="taskNamespace" type="xsd:string" minOccurs="0"/>
      <xsd:element name="actionDisplayName" type="xsd:string" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="timers" type="xsd:string" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="componentType" type="xsd:string" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="activityName" type="xsd:string" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="activityId" type="xsd:string" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="processDueDate" type="xsd:dateTime" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="thread" type="xsd:integer" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="parentThread" type="xsd:integer" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="step" type="xsd:integer" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="visibilityRuleConditionValues" type="xsd:string" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="isPartialTask" type="xsd:boolean" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="organizationalUnitId" type="xsd:string" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="tenantId" type="xsd:string" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="listBuilderId" type="xsd:string" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="caseId" type="xsd:string" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="organizationalUnitName" type="xsd:string" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="swimlaneRole" type="xsd:string" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="excludedParticipants" type="xsd:string" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="timersSuspended" type="xsd:boolean" minOccurs="0" maxOccurs="1" default="false"/>
      <xsd:element name="tenantIdNumber" type="xsd:long" minOccurs="0" maxOccurs="1" default="-1"/>
      <xsd:element name="taskAgeFromCreatedDate" type="xsd:duration" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="taskAgeFromAssignedDate" type="xsd:duration" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="taskTimeRemaining" type="xsd:duration" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="isDecomposedTask" type="xsd:boolean" minOccurs="0" maxOccurs="1" default="false"/>
      <xsd:element name="formName" type="xsd:string" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="decomposedTaskGroupInstanceId" type="xsd:string" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="startTime" type="xsd:dateTime" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="stopTime" type="xsd:dateTime" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="workingDuration" type="xsd:integer" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="acquiredByDisplayName" type="xsd:string" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="originalAssigneeUserDisplayName" type="xsd:string" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="approversDisplayNames" type="xsd:string" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="imageUrl" type="xsd:string" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="hiddenAttributes" type="tns:hiddenAttributesType" minOccurs="0" maxOccurs="1"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:element name="collectionTarget" type="tns:collectionTargetType"/>
  <xsd:complexType name="collectionTargetType">
    <xsd:sequence>
      <xsd:element name="id" type="xsd:string" minOccurs="1" maxOccurs="1"/>
      <xsd:element name="xpath" type="xsd:string" minOccurs="1" maxOccurs="1"/>
      <xsd:element name="collectionName" type="xsd:string" minOccurs="1" maxOccurs="1"/>
      <xsd:element name="type" type="tns:collectionTargetTypeEnum" minOccurs="1" maxOccurs="1"/>
      <xsd:choice minOccurs="1" maxOccurs="1">
        <xsd:element name="targetIndex" type="xsd:integer"/>
        <xsd:element ref="tns:keyList"/>
      </xsd:choice>
      <xsd:element name="referencedTaskId" type="xsd:string" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="taskAggregationId" type="xsd:string" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="action" type="tns:collectionTargetActionType" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="status" type="xsd:string" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="displayName" type="xsd:string" minOccurs="0" maxOccurs="1"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:simpleType name="collectionTargetTypeEnum">
