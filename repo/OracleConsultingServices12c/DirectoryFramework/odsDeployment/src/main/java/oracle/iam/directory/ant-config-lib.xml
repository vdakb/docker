<antlib xmlns:server="http://oracle.ods.deployment/config">
  <!--
  ==============================================================================
  == Register Custom Types within Ant
  ==============================================================================
  -->
  <typedef name="bind"      classname="oracle.iam.directory.config.type.Bind"/>
  <typedef name="action"    classname="oracle.iam.directory.config.type.Action"/>
  <typedef name="option"    classname="oracle.iam.directory.config.type.Option"/>
  <typedef name="command"   classname="oracle.iam.directory.config.type.Command"/>
  <typedef name="property"  classname="oracle.iam.directory.config.type.Property"/>
  <typedef name="parameter" classname="oracle.iam.directory.common.type.Parameter"/>
  <!--
  ==============================================================================
  == Register Custom Tasks within Ant
  ==============================================================================
  -->
  <taskdef name="server"    classname="oracle.iam.directory.config.task.Server"/>
  <taskdef name="suffix"    classname="oracle.iam.directory.config.task.Suffix"/>
</antlib>