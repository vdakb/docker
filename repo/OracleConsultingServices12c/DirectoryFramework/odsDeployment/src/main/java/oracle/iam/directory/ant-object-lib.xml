<antlib xmlns:server="http://oracle.ods.deployment/object">
  <!--
  ==============================================================================
  == Register Custom Types within Ant
  ==============================================================================
  -->
  <typedef name="name"        classname="oracle.iam.directory.common.type.Name"/>
  <typedef name="search"      classname="oracle.iam.directory.common.type.Search"/>
  <typedef name="binaries"    classname="oracle.iam.directory.common.type.NameCollection"/>
  <typedef name="excluded"    classname="oracle.iam.directory.common.type.NameCollection"/>
  <typedef name="included"    classname="oracle.iam.directory.common.type.NameCollection"/>
  <typedef name="exportSet"   classname="oracle.iam.directory.object.type.ExportSet"/>
  <typedef name="importSet"   classname="oracle.iam.directory.object.type.ImportSet"/>
  <typedef name="importFile"  classname="oracle.iam.directory.object.type.ImportFile"/>
  <!--
  ==============================================================================
  == Register Custom Tasks within Ant
  ==============================================================================
  -->
  <taskdef name="export"      classname="oracle.iam.directory.object.task.Export"/>
  <taskdef name="import"      classname="oracle.iam.directory.object.task.Import"/>
</antlib>
