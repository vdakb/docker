<antlib xmlns:server="http://oracle.ods.deployment/entry">
  <!--
  ==============================================================================
  == Register Custom Types within Ant
  ==============================================================================
  -->
  <typedef name="value"       classname="oracle.iam.directory.common.type.Value"/>
  <typedef name="entry"       classname="oracle.iam.directory.entry.type.Entry"/>
  <typedef name="attribute"   classname="oracle.iam.directory.entry.type.Attribute"/>
  <!--
  ==============================================================================
  == Register Custom Tasks within Ant
  ==============================================================================
  -->
  <taskdef name="create"      classname="oracle.iam.directory.entry.task.Create"/>
  <taskdef name="modify"      classname="oracle.iam.directory.entry.task.Modify"/>
  <taskdef name="delete"      classname="oracle.iam.directory.entry.task.Delete"/>
  <taskdef name="rename"      classname="oracle.iam.directory.entry.task.Rename"/>
  <taskdef name="move"        classname="oracle.iam.directory.entry.task.Move"/>
</antlib>