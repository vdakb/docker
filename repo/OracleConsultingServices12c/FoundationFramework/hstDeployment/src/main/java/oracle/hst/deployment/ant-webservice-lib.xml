<antlib xmlns:object="http://oracle.hst.deployment/webservice">
  <!--
  ==============================================================================
  == Register Custom Types within Ant
  ==============================================================================
  -->
  <typedef name="policy"    classname="oracle.hst.deployment.wsm.type.Policy"/>
  <typedef name="overrides" classname="oracle.hst.deployment.wsm.type.Overrides"/>
  <typedef name="server"    classname="oracle.hst.deployment.wsm.type.Service$Server"/>
  <typedef name="client"    classname="oracle.hst.deployment.wsm.type.Service$Client"/>
  <typedef name="report"    classname="oracle.hst.deployment.wsm.type.Operation$Report"/>
  <typedef name="attach"    classname="oracle.hst.deployment.wsm.type.Operation$Attach"/>
  <typedef name="detach"    classname="oracle.hst.deployment.wsm.type.Operation$Detach"/>
  <typedef name="enable"    classname="oracle.hst.deployment.wsm.type.Operation$Enable"/>
  <typedef name="disable"   classname="oracle.hst.deployment.wsm.type.Operation$Disable"/>
  <!--
  ==============================================================================
  == Register Custom Tasks within Ant
  ==============================================================================
  -->
  <taskdef name="security"  classname="oracle.hst.deployment.wsm.task.WebService"/>
</antlib>