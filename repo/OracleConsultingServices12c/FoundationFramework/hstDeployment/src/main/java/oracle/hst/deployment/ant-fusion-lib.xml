<antlib xmlns:object="http://oracle.hst.deployment/fusion">
  <!--
  ==============================================================================
  == Register Custom Types within Ant
  ==============================================================================
  -->
  <typedef name="service"   classname="oracle.hst.deployment.type.JMXManagedService"/>

  <typedef name="domain"    classname="oracle.hst.deployment.fmw.type.Domain"/>
  <typedef name="type"      classname="oracle.hst.deployment.fmw.type.Type"/>
  <typedef name="bean"      classname="oracle.hst.deployment.fmw.type.Bean"/>
  <typedef name="attribute" classname="oracle.hst.deployment.fmw.type.Bean$Attribute"/>
  <typedef name="property"  classname="oracle.hst.deployment.fmw.type.Bean$Property"/>
  <typedef name="operation" classname="oracle.hst.deployment.fmw.type.Bean$Operation"/>
  <typedef name="parameter" classname="oracle.hst.deployment.fmw.type.Bean$Parameter"/>
  <!--
  ==============================================================================
  == Register Custom Tasks within Ant
  ==============================================================================
  -->
  <taskdef name="actual"    classname="oracle.hst.deployment.fmw.task.Environment$Actual"/>
  <taskdef name="compare"   classname="oracle.hst.deployment.fmw.task.Environment$Compare"/>
  <taskdef name="invoke"    classname="oracle.hst.deployment.fmw.task.Environment$Invoke"/>
  <taskdef name="modify"    classname="oracle.hst.deployment.fmw.task.Environment$Modify"/>
</antlib>