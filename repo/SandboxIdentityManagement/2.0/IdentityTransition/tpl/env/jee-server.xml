<?xml version="1.0" encoding="US-ASCII"?>
<project name="Application Server Properties" basedir=".">
  <!--
  =====================================================================
  == These properties specifies where the remote runtime environment of
  == a JEE Server is located
  =====================================================================
  -->
  <property name="jee.server.type"     value="weblogic"/>
  <property name="jee.server.protocol" value="__JEE_PROT__"/>
  <property name="jee.server.host"     value="__JEE_HOST__"/>
  <property name="jee.server.port"     value="__JEE_PORT__"/>
  <property name="jee.server.name"     value="__JEE_NAME__"/>
  <property name="jee.server.username" value="__JEE_USER__"/>
  <!--
  Password must be plain text. To avoid storing the server password in
  this script, it is stored in a wallet instead.
  -->
  <property name="jee.server.password" value="__JEE_PASS__"/>
  <property name="jee.server.home"     value="__JEE_HOME__"/>
</project>
