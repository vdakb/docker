<?xml version="1.0" encoding="US-ASCII"?>
<project name="Federation Directory Server Properties" basedir=".">
  <!--
  ==============================================================================
  == These properties specifies where the remote runtime environment of
  == Oracle Identity Manager local conext is located
  ==============================================================================
  -->
  <property name="fed.server.type"       value="internetDirectory"/>
  <property name="fed.server.protocol"   value="__FED_PROT__"/>
  <property name="fed.server.host"       value="__FED_HOST__"/>
  <property name="fed.server.port"       value="__FED_PORT__"/>
  <property name="fed.server.context"    value="__FED_ROOT__"/>
  <property name="fed.server.username"   value="__FED_USER__"/>
  <property name="fed.server.password"   value="__FED_PASS__"/>
  <property name="fed.server.instance"   value="__FED_PATH__"/>
  <!--
  =====================================================================
  == These properties specifies where the remote administration
  == environment of a Direcory Server is located
  =====================================================================
  -->
  <property name="fed.admin.port"        value="__ADM_PORT__"/>
  <property name="fed.admin.username"    value="__ADM_USER__"/>
  <property name="fed.admin.password"    value="__ADM_PASS__"/>
  <property name="fed.admin.credential"  value="__ADM_AUTH__"/>
  <!--
  ==============================================================================
  == These properties specifies the credentials of administrative
  == accounts.
  ==============================================================================
  -->
  <property name="iam.server.password"  value="__IAM_PASS__"/>
  <property name="oim.server.password"  value="__OIM_PASS__"/>
  <property name="oam.server.password"  value="__OAM_PASS__"/>
</project>