<?xml version="1.0" encoding="US-ASCII"?>
<project name="Oracle Internet Directory Server Properties" basedir=".">
  <!--
  ==============================================================================
  == These properties specifies where the remote runtime environment of
  == Oracle Identity Manager local conext is located
  ==============================================================================
  -->
  <property name="oid.server.type"       value="internetDirectory"/>
  <property name="oid.server.protocol"   value="__OID_PROT__"/>
  <property name="oid.server.host"       value="__OID_HOST__"/>
  <property name="oid.server.port"       value="__OID_PORT__"/>
  <property name="oid.server.context"    value="__OID_ROOT__"/>
  <property name="oid.server.username"   value="__OID_USER__"/>
  <property name="oid.server.password"   value="__OID_PASS__"/>
  <property name="oid.server.instance"   value="__OID_PATH__"/>
  <!--
  ==============================================================================
  == These properties specifies the credentials of administrative
  == accounts.
  ==============================================================================
  -->
  <property name="iam.server.password"  value="__IAM_PASS__"/>
  <property name="oim.server.password"  value="__OIM_PASS__"/>
  <property name="oam.server.password"  value="__OAM_PASS__"/>
</project>